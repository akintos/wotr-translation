msgid "Voice of the Cursed Bard"
msgstr "저주받은 음유시인의 목소리"

#. Type = Kingmaker.Blueprints.Items.BlueprintItem
#. Path = Relic_VocalCord::m_DescriptionText
#. 
#. Type = Kingmaker.Blueprints.Items.Equipment.BlueprintItemEquipmentNeck
#. Path = CursedBardVoiceAmulet_IntermediateItem::m_FlavorText
#. 
#. Type = Kingmaker.Blueprints.Items.Equipment.BlueprintItemEquipmentNeck
#. Path = CursedBardVoiceAmulet_SirenItem::m_FlavorText
#. 
#. Type = Kingmaker.Blueprints.Items.Equipment.BlueprintItemEquipmentNeck
