#. Type = Kingmaker.Blueprints.Items.BlueprintItem
#. Path = m_DescriptionText
msgctxt "de0777d1-4927-47da-9bea-347e3b93c8c8"
msgid "Some notes written in ancient Elven runic script."
msgstr "고대 엘프 룬 문자로 적혀진 문서들 입니다."

#. File = Items/Archeology/LexiconPages/LexiconPageFromGG
#. Type = Kingmaker.Blueprints.Items.BlueprintItem
#. Path = m_DisplayNameText
msgctxt "00d1cb86-a93c-4844-b6cd-832b901a8b61"
