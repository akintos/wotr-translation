#. File = Equipment/Gloves/PF2/CrusadeRelics/Chapter5/BaphometFire/BaphometFireGloves_AnimalisticFireItem
#. Type = Kingmaker.Blueprints.Items.Equipment.BlueprintItemEquipmentGloves
#. Path = m_DescriptionText
#. 
#. File = Equipment/Gloves/PF2/CrusadeRelics/Chapter5/BaphometFire/BaphometFireGloves_AnimalisticFireToggleAbilityMount
#. Type = Kingmaker.UnitLogic.ActivatableAbilities.BlueprintActivatableAbility
