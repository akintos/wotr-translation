#. File = Items/Crusade/Relic_SymbolOfRovagug
#. Type = Kingmaker.Blueprints.Items.BlueprintItem
#. Path = m_DescriptionText
#. 
#. File = Weapons/Items/UniquePF2/CrusadeRelics/Chapter2/Rovagug/RovagugRelicBattlaxe_IntermediateItem
#. Type = Kingmaker.Blueprints.Items.Weapons.BlueprintItemWeapon
