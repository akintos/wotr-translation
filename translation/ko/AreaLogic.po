msgctxt "ea1dddfb-713d-4f5d-b51d-f5931774670a"
msgid "\"There is no release... No freedom...\""
msgstr "\"해방은 없어... 자유는 없어...\""

#. File = World/Cutscenes/DLC5_PlaneOfShadow_Basement_2/DLC5_LC_ExecutorSacri/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "7c838d29-8d6f-494e-80db-dfe14bc7e188"
msgid "\"Come on! Pull the lever!\""
msgstr "\"이리와! 레버를 당겨!\""

