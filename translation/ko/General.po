#. Type = Kingmaker.Globalmap.Blueprints.BlueprintGlobalMapPointVariation
#. Path = Name
msgctxt "55a3f520-d804-4a4d-9100-679e563850ce"
msgid "Kenabres"
msgstr "케나브레스"

#. File = GlobalMaps/WorldWoundGM/Points/Location_Lann_Q3_SavamelekhLair
#. Type = Kingmaker.Globalmap.Blueprints.BlueprintGlobalMapPoint
#. Path = Description
#. 
#. File = GlobalMaps/WorldWoundGM/Points/Location_Lann_Q3_SavamelekhLair_Lann
#. Type = Kingmaker.Globalmap.Blueprints.BlueprintGlobalMapPointVariation
