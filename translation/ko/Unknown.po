msgstr "내려가기"

msgctxt "01f199bc-e59c-4ffc-8e8f-79428e4d7a80"
msgid "Ascend to the top"
msgstr "꼭대기로 올라가기"

msgctxt "1851cf92-c134-4ea3-9619-b6c37a3f5d0d"
