msgctxt "d053bd2d-48b8-4c45-ad8b-135311757f52"
msgid "\"We'll win!\""
msgstr "\"우린 이길 것이다!\""

#. File = World/Encounters/DrezenCitadel_Level1/ActionsHolders/HiddenSolderBarks
#. Type = Kingmaker.ElementsSystem.ActionsHolder
#. Path = Actions/Actions[0]/Actions[4]/Action/Actions[0]/WhatToBark
