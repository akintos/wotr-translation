#. Type = Kingmaker.UnitLogic.Buffs.Blueprints.BlueprintBuff
#. Path = m_DisplayName
msgctxt "69114a7a-4421-4b6e-941e-c225ad1569e0"
msgid "Snowball — Staggered"
msgstr "눈뭉치 — 비틀거림"

#. File = Spells/Level1/StoneFist
#. Type = Kingmaker.UnitLogic.Abilities.Blueprints.BlueprintAbility
#. Path = m_Description
#. 
#. File = Spells/Level1/StoneFistBuffMedium
#. Type = Kingmaker.UnitLogic.Buffs.Blueprints.BlueprintBuff
