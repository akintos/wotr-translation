msgid "Contagion"
msgstr "전염"

#. File = Spells/Level3/ContagionBlindingSickness
#. Type = Kingmaker.UnitLogic.Abilities.Blueprints.BlueprintAbility
#. Path = m_Description
#. 
#. File = Spells/Level3/ContagionBlindingSicknessResolve
#. Type = Kingmaker.UnitLogic.Abilities.Blueprints.BlueprintAbility
