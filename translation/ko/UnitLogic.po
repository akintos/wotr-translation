#. Path = BaphometFireGloves_AnimalisticFireToggleAbilitySelf::m_DisplayName
msgctxt "61545e4d-769d-4ef1-b2d1-1cc98c140608"
msgid "Call of the Fiery Things — Self"
msgstr ""

#. Type = Kingmaker.UnitLogic.ActivatableAbilities.BlueprintActivatableAbility
#. Path = BlindingCriticalToggleAbility::m_DisplayName
msgctxt "6f69a134-b366-4c4c-a282-72b91a863a2d"
msgid "Blinding Critical"
msgstr "실명 치명타"

#. Type = Kingmaker.UnitLogic.ActivatableAbilities.BlueprintActivatableAbility
#. Path = BloodlineElementalAirArcanaAbilily::m_DisplayName
#. 
#. Type = Kingmaker.UnitLogic.Buffs.Blueprints.BlueprintBuff
#. Path = BloodlineElementalAirArcanaBuff::m_DisplayName
msgctxt "275662d4-3919-4de2-b8d6-476cdba7720d"
msgid "Elemental Bloodline Arcana — Air"
msgstr "원소 혈통 비의 — 대기"

#. Type = Kingmaker.UnitLogic.ActivatableAbilities.BlueprintActivatableAbility
#. Path = BloodlineElementalAirArcanaAbilily::m_Description
#. 
#. Type = Kingmaker.UnitLogic.Buffs.Blueprints.BlueprintBuff
#. Path = BloodlineElementalAirArcanaBuff::m_Description
msgctxt "6d47d9fc-38d4-4389-b0fb-8bb4c5090564"
msgid ""
"All {g|Encyclopedia:Spell}spells{/g} that deal {g|Encyclopedia:Energy_Damage}"
"energy damage{/g} that you cast will deal electricity "
"{g|Encyclopedia:Damage}damage{/g} instead. This also changes the spell's "
