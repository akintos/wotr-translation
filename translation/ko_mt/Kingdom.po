msgid "{g|NemaryStigma_Bardiche_MagicSealing}[Choice effects]{/g} The druid"
msgstr "{g|NemaryStigma_Bardiche_MagicSealing}[선택 효과]{/g} 드루이드"

#. File = World/Crusade/Events/Relics/Ch4/NemaryStigma_Enchanting_event
#. Type = Kingmaker.Kingdom.Blueprints.BlueprintCrusadeEvent
#. Path = m_EventSolutions[1]/m_ResultText
