#. Type = Kingmaker.AreaLogic.Cutscenes.CommandAction
#. Path = Action/Actions[0]/Actions[2]/Action/Actions[0]/WhatToBark
#. 
#. File = World/Cutscenes/DLC5_LostChapel_PlaneOfShadow/DLC5_LC_ShadowsCutscene/CommandAction2
#. Type = Kingmaker.AreaLogic.Cutscenes.CommandAction
#. Path = Action/Actions[0]/Actions[3]/Action/Actions[0]/WhatToBark
msgctxt "ea1dddfb-713d-4f5d-b51d-f5931774670a"
msgid "\"There is no release... No freedom...\""
msgstr "\"해방은 없다... 자유도 없고...\""

#. File = World/Cutscenes/DLC5_PlaneOfShadow_Basement_2/DLC5_LC_ExecutorSacri/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "7c838d29-8d6f-494e-80db-dfe14bc7e188"
msgid "\"Come on! Pull the lever!\""
msgstr "\"어서! 레버를 당겨!\""

#. File = CreatureAbilities/Templates/Vampire/VampireEnergyDrain
#. Type = Kingmaker.Blueprints.Classes.BlueprintFeature
#. Path = Components[0]/Action/Actions[0]/IfFalse/Actions[0]/ConditionsChecker/Conditions[0]/m_Difficulty/NormalDifficulty/LocalizedDescription
#. 
#. File = CreatureAbilities/Templates/Vampire/VampireSpawnEnergyDrain
#. Type = Kingmaker.Blueprints.Classes.BlueprintFeature
#. Path = Components[0]/Action/Actions[0]/IfFalse/Actions[0]/ConditionsChecker/Conditions[0]/m_Difficulty/NormalDifficulty/LocalizedDescription
#. 
