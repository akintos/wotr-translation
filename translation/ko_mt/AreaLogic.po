msgid "Normal"
msgstr "보통"

#. File = World/Cutscenes/DLC5_SithhudLair/DLC5_SithhudReadyToFall/CommandBark3
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "c06bb3b1-0a9a-4054-9c76-84947f4d4907"
msgid "\"I can survive even death... Can you?\""
msgstr "\"나는 죽음에서도 살아남을 수 있습니다... 당신은? \""

#. File = World/Cutscenes/DLC5_SithhudLair/DLC5_Sithhud_first_whisper/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "14d862b2-7c5b-4843-882f-b72eef027040"
msgid "\"Who... Who are you?\""
msgstr "\"누구.. 누구세요?\""

#. File = World/Cutscenes/DLC5_SithhudLair/DLC5_Sithhud_is_finally_dead/CommandBark5
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "d14fce12-4260-4ac5-a93f-2dabcd3c5f6a"
msgid "\"Was it... All... In vain?\""
msgstr "\"Was.. 모두.. 헛수고였나요?\""

#. File = World/Cutscenes/DLC5_SithhudLair/DLC5_Sithhud_second_whisper/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "274f8c39-0a4a-4de9-bb54-e631543bf417"
msgid "\"Closer... Come closer...\""
msgstr "\"가까이.. 더 가까이...\""

#. File = World/Cutscenes/DLC5_Storasta/DLC5_CheeringPeople_AfterKarrok/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
#. 
#. File = World/Cutscenes/DLC5_Storasta/DLC5_Storasta_SendriPortal/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "08a17065-6957-4589-8ca4-dba0d1f8097b"
msgid "\"Hear ye, hear ye! Carrock is dead! We are free!\""
msgstr "\"들으라, 들으라! 캐록이 죽었다! 우린 자유다!\""

#. File = World/Cutscenes/DLC5_Storasta/DLC5_CheeringPeople_AfterKarrok/CommandBark1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
#. 
#. File = World/Cutscenes/DLC5_Storasta/DLC5_Storasta_SendriPortal/CommandBark1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "dffd1825-3c6e-4055-aebb-4a0c001631ec"
msgid "\"Where are the crusaders who came to our aid? There they are!\""
msgstr "\"우리를 도와주러 온 십자군은 어디 있지? 저기 있네!\""

#. File = World/Cutscenes/DLC5_Storasta/DLC5_CheeringPeople_AfterKarrok/CommandBark2
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
#. 
#. File = World/Cutscenes/DLC5_Storasta/DLC5_Storasta_SendriPortal/CommandBark2
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "92886056-6124-4dfd-bd7f-aed1043990bf"
msgid "\"We've made it? What are the chances!\""
msgstr "\"우리가 해냈어요? 이게 무슨 일이야!\""

#. File = World/Cutscenes/DLC5_Storasta/DLC5_CheeringPeople_AfterKarrok/CommandBark3
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
#. 
#. File = World/Cutscenes/DLC5_Storasta/DLC5_Storasta_SendriPortal/CommandBark3
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "5f0b5932-26d6-40fb-8500-048852e5801a"
msgid "\"Thank you! We will never forget your heroism.\""
msgstr "\"감사합니다! 우리는 당신의 영웅심을 결코 잊지 않을 것입니다.\""

#. File = World/Cutscenes/DLC5_Storasta/DLC5_CheeringPeople_AfterKarrok/CommandBark4
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
#. 
#. File = World/Cutscenes/DLC5_Storasta/DLC5_Storasta_SendriPortal/CommandBark4
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "5e23cdad-6960-4f09-9f27-4af6c892087e"
msgid ""
"\"Oh, it would be wonderful to stay here and celebrate our victory… But "
"alas, there is no time. We must press on.\""
msgstr "\"여기 남아서 우리의 승리를 축하하고 싶지만 아쉽게도 시간이 없습니다. 우리는 "
"계속 나아가야 합니다.\""

#. File = World/Cutscenes/DLC5_Storasta/DLC5_CheeringPeople_AfterKarrok/CommandBark5
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
#. 
#. File = World/Cutscenes/DLC5_Storasta/DLC5_Storasta_SendriPortal/CommandBark5
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "46f7c11c-9065-4a96-baa4-b901c2af7ffd"
msgid "\"We must go as well. Farewell! May the gods guide you!\""
msgstr "\"우리도 가야 합니다. 잘 가라! 신이 인도하길!\""

#. File = World/Cutscenes/DLC5_Storasta/DLC5_PlantsPool/CommandAction
#. Type = Kingmaker.AreaLogic.Cutscenes.CommandAction
#. Path = Action/Actions[0]/WhatToBark
msgctxt "1822d17e-d218-4be5-a75e-cc8377534373"
msgid "\"Approach, if your intentions are peaceful.\""
msgstr "\"평화로운 의도를 가지고 있다면 접근하세요.\""

#. File = World/Cutscenes/DLC5_Storasta/DLC5_Storasta_AfterMet/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "f03350d2-d03d-412c-a4bf-ac6ccc3220ac"
msgid "\"Right, that's it. Show's over, let's go.\""
msgstr "\"좋아, 끝났어. 쇼가 끝났으니 가자.\""

#. File = World/Cutscenes/DLC5_Storasta/DLC5_Storasta_Ambush/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "8f7b3087-9f83-44a4-aa9e-2316a3963693"
msgid "\"I feel uncomfortable… As if someone is watching us!\""
msgstr "\"누군가 우리를 지켜보는 것 같아 불편해요!\""

#. File = World/Cutscenes/DLC5_Storasta/DLC5_Storasta_Ambush/CommandBark1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
#. 
#. File = World/Cutscenes/DLC5_Storasta/DLC5_Storasta_NymphAmbush/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "6b1e6289-7f07-40e2-95b7-e5d397e18ee6"
msgid "\"Mortals? Have you come here out of malice, or foolishness?\""
msgstr "\"필멸자들? 악의나 어리석음 때문에 여기 온 건가?\""

#. File = World/Cutscenes/DLC5_Storasta/DLC5_Storasta_Ambush/CommandBark2
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
#. 
#. File = World/Cutscenes/DLC5_Storasta/DLC5_Storasta_Ambush/CommandBark3
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
#. 
#. File = World/Cutscenes/DLC5_Storasta/DLC5_Storasta_NymphAmbush/CommandBark2
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
#. 
#. File = World/Cutscenes/DLC5_Storasta/DLC5_Storasta_NymphAmbush/CommandBark4
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "fe8eef07-558b-4321-9f7e-1646c3046005"
msgid ""
"\"There, there, sister. They're just mortals. Hey, you, what do you want?\""
msgstr "\"저기, 저기, 자매님. 그냥 인간일 뿐이야. 이봐, 너, 원하는 게 뭐야?\""

#. File = World/Cutscenes/DLC5_Storasta/DLC5_Storasta_Ambush/CommandBark4
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "60ebb8e2-2383-4e4a-bdf4-8113429dbd43"
msgid "\"Mortals! They're so funny!\""
msgstr "\"필멸자들! 너무 웃겨요!\""

#. File = World/Cutscenes/DLC5_Storasta/DLC5_Storasta_Ambush/CommandBark5
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
#. 
#. File = World/Cutscenes/DLC5_Storasta/DLC5_Storasta_NymphAmbush/CommandBark3
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
#. 
#. File = World/Cutscenes/DLC5_Storasta/DLC5_Storasta_NymphHelp/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "b623b327-406f-43cc-8386-7136df18f331"
msgid "\"Outsiders! Monsters! To arms, sisters!\""
msgstr "\"외부인! 괴물들이다! 무기를 들어라, 자매들아!\""

#. File = World/Cutscenes/DLC5_Storasta/DLC5_Storasta_BuletteBark/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "721f4588-2240-4c2b-9994-f18502a77844"
msgid "\"Help! Do something!\""
msgstr "\"도와주세요! 어떻게 좀 해줘요!\""

#. File = World/Cutscenes/DLC5_Storasta/DLC5_Storasta_BurryDruid/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "a3ef433c-0e4c-4497-98ab-0767d92d26a7"
msgid ""
"\"I am free! You have my thanks, please take my best equipment as a token of "
"gratitude. It is in my grave. Bless you…\""
msgstr "\"나는 자유다! 감사의 표시로 내 최고의 장비를 가져가세요. 내 무덤에 있어. "
"축복을...\""

#. File = World/Cutscenes/DLC5_Storasta/DLC5_Storasta_DeadDruidsCall/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "92709036-98af-4ebe-8554-4cf3679afcfa"
msgid "\"None of us shall rest while the mad Carrock lives…\""
msgstr "\"미친 캐록이 살아있는 한 우리 중 누구도 쉬지 않으리...\""

#. File = World/Cutscenes/DLC5_Storasta/DLC5_Storasta_DragonAttack/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "5553d2fe-5d87-4723-b4f6-7be1de704522"
msgid ""
"\"What are you doing? Are you mad, or traitors? No matter. Attack them!\""
msgstr "\"뭐 하는 거야? 미쳤어? 아니면 배신자야? 상관없어. 공격해!\""

#. File = World/Cutscenes/DLC5_Storasta/DLC5_Storasta_DragonAttack/CommandBark1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "76327e05-c437-4240-8bc7-d98b7a3a94cb"
msgid "\"Have at him! That lizard had it coming!\""
msgstr "\"저놈을 잡아라! 저 도마뱀이 당할 줄 알았어!\""

#. File = World/Cutscenes/DLC5_Storasta/DLC5_Storasta_DragonCome/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "a292246f-75a9-414d-a8f8-0c4a8f939c86"
msgid "\"Look! It's the dragon!\""
msgstr "\"저기 봐요, 용이에요!\""

#. File = World/Cutscenes/DLC5_Storasta/DLC5_Storasta_DrakeCutscene/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "60ab884b-e26a-4c3e-87fb-e74e07135759"
msgid "\"Wow! What's he doing here?\""
msgstr "\"와! 저 사람이 여기서 뭐 하는 거야?\""

#. File = World/Cutscenes/DLC5_Storasta/DLC5_Storasta_FirstSZ/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
#. 
#. File = World/Cutscenes/DLC5_Storasta/DLC5_Storasta_FowBridge/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "9d9ac789-bf31-44e1-aed4-03fbe72dbb93"
msgid ""
"\"Can you hear it? What is it, a concert in the middle of the woods? Sounds "
"like drums.\""
msgstr "\"들려요? 숲 한가운데서 무슨 콘서트 소리인가요? 드럼 소리 같네요.\""

#. File = World/Cutscenes/DLC5_Storasta/DLC5_Storasta_KarokkHill/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "9256fc64-c8cb-4afb-b38f-8b00795e03dc"
msgid ""
"\"We're in the very heart of this blighted forest. Stay vigilant. Carrock is "
"nearby!\""
msgstr "\"우리는 이 황폐한 숲의 한가운데에 있습니다. 경계를 늦추지 마세요. 캐록이 "
"근처에 있다!\""

#. File = World/Cutscenes/DLC5_Storasta/DLC5_Storasta_KarrokMadness/CommandAction
#. Type = Kingmaker.AreaLogic.Cutscenes.CommandAction
#. Path = Action/Actions[0]/Actions[1]/Action/Actions[0]/WhatToBark
msgctxt "56fe1218-c892-4ccf-bee6-c840accbc0cc"
msgid "\"No foe shall pass… No foe shall dare… Woe to the foes of Storasta…\""
msgstr "\"어떤 적도 통과하지 못하리... 어떤 적도 감히... 스토라스타의 적들에게 화가 "
"있으리라...\""

#. File = World/Cutscenes/DLC5_Storasta/DLC5_Storasta_KarrokMadness/CommandAction
#. Type = Kingmaker.AreaLogic.Cutscenes.CommandAction
#. Path = Action/Actions[0]/Actions[2]/Action/Actions[0]/WhatToBark
msgctxt "f8a1adc6-f370-4035-8270-3f3b766c005c"
msgid "\"Keep out… Trample underfoot… Destroy…\""
msgstr "\"막아라... 발로 짓밟아라... 파괴하라...\""

#. File = World/Cutscenes/DLC5_Storasta/DLC5_Storasta_KarrokMadness/CommandAction
#. Type = Kingmaker.AreaLogic.Cutscenes.CommandAction
#. Path = Action/Actions[0]/Actions[0]/Action/Actions[0]/WhatToBark
msgctxt "3d08b0fd-2adc-4cde-bcdc-daf335fdb958"
msgid "\"Storasta… I guard Storasta…\""
msgstr "\"스토라스타... 나는 스토라스타를 지킨다...\""

#. File = World/Cutscenes/DLC5_Storasta/DLC5_Storasta_Movepoint/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "1ff8d74e-9fd9-475b-8ca3-bc018182ff52"
msgid "\"What happened? Where are the others?\""
msgstr "\"무슨 일이야? 다른 사람들은 어딨어?\""

#. File = World/Cutscenes/DLC5_Storasta/DLC5_Storasta_Movepoint/CommandBark1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "16ba18ab-676b-4ff4-903d-f4b191a219a5"
msgid ""
"\"A bulette attacked us, jumped straight up from underground. Everyone else "
"died. Let us pass or we'll join them.\""
msgstr "\"불렛이 우리를 공격했고 지하에서 곧장 뛰어올랐어요. 다른 사람들은 모두 "
"죽었어요. 우릴 통과시키지 않으면 합류하겠다.\""

#. File = World/Cutscenes/DLC5_Storasta/DLC5_Storasta_NymphBack/CommandAction
#. Type = Kingmaker.AreaLogic.Cutscenes.CommandAction
#. Path = Action/Actions[0]/WhatToBark
msgctxt "607e71b5-d958-4302-9950-ddb9b0502888"
msgid ""
"\"Why would you wander here? You're mortals. You might get… Murded? "
"Murdurded? Murdened… It's dangerous here, you know!\""
msgstr ""
"\"왜 여기서 방황하는 거죠? 너희는 필멸자잖아. 죽을지도 모르는데... "
"살해당할까봐? 살해당한다고? 살해당해... 여긴 위험해, 알잖아!\""

#. File = World/Cutscenes/DLC5_Storasta/DLC5_Storasta_PrepareToFight/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "48e7f8dc-66a7-448a-9657-a9c1e803b0b4"
msgid "\"Onwards, crusaders! Let's claim our freedom!\""
msgstr "\"전진하라, 십자군! 우리의 자유를 쟁취하자!\""

#. File = World/Cutscenes/DLC5_Storasta/DLC5_Storasta_SendriPortal/CommandBark6
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "e7b1f5e2-740e-41f8-a6fa-7db9c6cef177"
msgid "\"It's time for us to move along.\""
msgstr "\"이제 우리가 함께 나아갈 때입니다.\""

#. File = World/Cutscenes/DLC5_Storasta/DLC5_Storasta_SquareCutscene/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "7c5fd68d-aee2-4925-aacd-4d9c4e484ac5"
msgid ""
"\"That's right. Our valiant ancestors fought the dragon, but faced with the "
"monsters from the woods, they made a pact. So our village was founded.\""
msgstr ""
"\"맞아요. 우리의 용감한 조상들은 용과 싸웠지만 숲에서 온 괴물들과 맞서 "
"싸우다가 협정을 맺었죠. 그래서 우리 마을이 세워졌죠.\""

#. File = World/Cutscenes/DLC5_Storasta/DLC5_Storasta_SquareCutscene/CommandBark1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "cca8bd04-ffe5-4736-9b5c-d71bf92d9804"
msgid ""
"\"Stop with that nonsense! If it wasn't for the dragon we'd all have been "
"eaten long ago.\""
msgstr "\"말도 안 되는 소리 그만해! 용만 아니었다면 우린 오래전에 다 잡아먹혔을 거야."
"\""

#. File = World/Cutscenes/DLC5_Storasta/DLC5_Storasta_SquareCutscene/CommandBark2
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "ea8248cb-312d-41ce-a31a-01b89a51ef45"
msgid "\"Pah! Flesh-eating bushes, dragons, they're all monsters to me!\""
msgstr "\"파! 살을 먹는 덤불, 용, 나에겐 모두 괴물이야!\""

#. File = World/Cutscenes/DLC5_Storasta/DLC5_Storasta_SquareCutscene/CommandBark3
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "04b2b4a2-edac-4601-a24b-02b82529feae"
msgid ""
"\"If it wasn't for the dragon our ancestors wouldn't have come to this "
"damned forest in the first place.\""
msgstr "\"용이 아니었다면 우리 조상들은 애초에 이 저주받은 숲에 오지 않았을 것입니다."
"\""

#. File = World/Cutscenes/DLC5_Storasta/DLC5_Storasta_SquareCutscene/CommandBark4
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "0496c176-0d80-4ddc-a70f-c76635a86e32"
msgid ""
"\"Hey, look! Who's that? They're dressed funny and they don't look much like "
"us… How did they get here?\""
msgstr "\"저기 봐요! 저게 누구야? 옷도 이상하고 우리랑 별로 안 닮았는데... 어떻게 "
"여기까지 왔지?\""

#. File = World/Cutscenes/DLC5_Storasta/DLC5_Storasta_Stop/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "b470782b-335a-453a-9162-542fe3b555ff"
msgid "\"Hey, where are you going? The chief is waiting for you!\""
msgstr "\"이봐요, 어디 가요? 서장님이 기다리신다!\""

#. File = World/Cutscenes/DLC5_Storasta/DLC5_Storasta_WarlordApproved/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "4f73f0eb-1899-4cf0-8867-f4bdea98618b"
msgid "\"Watch out in the forest. Few come out alive.\""
msgstr "\"숲에서 조심하세요. 살아남는 사람은 거의 없다.\""

#. File = World/Cutscenes/DLC5_Storasta/DLC5_WarlordOut/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "62762b80-139a-4020-9bbc-b1cb3de46621"
msgid "\"We should return to the village. There is still much to do.\""
msgstr "\"마을로 돌아가야 합니다. 아직 할 일이 많아요.\""

#. File = World/Cutscenes/DLC5_Tavern/DLC5_Intro_wakeup/CommandAction1
#. Type = Kingmaker.AreaLogic.Cutscenes.CommandAction
#. Path = Action/Actions[0]/WhatToBark
msgctxt "467e772a-90de-4aa1-ae11-f440190f185e"
msgid ""
"\"Hey, you alright? You were turning in your sleep… Go walk it off, clear "
"your head. You'll feel better.\""
msgstr "\"이봐, 괜찮아? 자다가 뒤척였잖아... 가서 좀 걷고 머리 좀 식혀. 기분이 "
"나아질 거야.\""

#. File = World/Cutscenes/Daeran_Q1_KenabresMansion/Daeran_Q1_KenabresMansion_PartyHallEntrance/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "a0052566-19a0-4a04-bbbb-f0e76cc18a41"
msgid ""
"\"What a splendid occasion, Count! And this new Numerian elixir is quite "
"something!\""
msgstr "\"정말 멋진 날입니다, 백작님! 그리고 이 새로운 누메리아의 묘약은 정말 "
"대단하군요!\""

#. File = World/Cutscenes/Daeran_Q1_KenabresMansion/Daeran_Q1_KenabresMansion_PartyHallEntrance/CommandBark 1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "a0d58f45-5564-4b48-bc23-3348959a4922"
msgid "\"I s-smell... beauty...\""
msgstr "\"나는.. 아름다움의.. 냄새가...\""

#. File = World/Cutscenes/Daeran_Q1_KenabresMansion/Daeran_Q1_KenabresMansion_PartyHallEntrance/CommandBark 2
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "9c244528-65f8-4450-87f2-52ae22d8aaef"
msgid ""
"\"Wonderful, now brace yourself for the smell of your own blood, you ghastly "
"eyesore!\""
msgstr "\"좋아, 이제 네 피 냄새를 맡을 준비를 해라, 이 끔찍한 눈엣가시야!\""

#. File = World/Cutscenes/Daeran_Q1_KenabresMansion/Daeran_Q1_KenabresMansion_PartyHallEntrance/CommandBark 3
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "a009119d-6ac5-4cff-9317-bd22ccc1c9ce"
msgid "\"Oh, look! Such darling little creatures!\""
msgstr "\"오, 저기 봐요! 정말 사랑스러운 작은 생명체들이야!\""

#. File = World/Cutscenes/Daeran_Q1_KenabresMansion/Daeran_Q1_KenabresMansion_PartyHallEntrance/CommandBark 4
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "a53ad5f4-caf1-4b96-a50f-77e80f610559"
msgid ""
"\"I must be well and truly sozzled... I'm seeing freaks with horns, bah-ha!\""
msgstr "\"나 정말 취했나 봐요.. 뿔 달린 괴물들이 보여요, 바하!\""

#. File = World/Cutscenes/Daeran_Q1_KenabresMansion/Daeran_Q1_KenabresMansion_PartyHallEntrance/CommandBark 5
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "6bea237d-5990-43d4-8013-9d51a4f84962"
msgid "\"They're — they're demons! Help!\""
msgstr "\"저들은. 데몬예요! 도와줘요!\""

#. File = World/Cutscenes/Daeran_Q1_KenabresMansion/Daeran_Q1_KenabresMansion_PartyHallEntrance/CommandBark 6
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "74aadf70-f246-4364-a244-725b8e856368"
msgid ""
"\"So the demons attacked Kenabres after all! Desna, protect the poor people "
"of this city!\""
msgstr "\"결국 데몬들이 케나브레스를 공격했군! 데스나, 이 도시의 불쌍한 사람들을 "
"지켜줘!\""

#. File = World/Cutscenes/Daeran_Q2_HeavenDoorstep/Daeran_Q2_CrateFireball/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "280ff466-bd14-45be-a99d-92ea1da041d4"
msgid ""
"\"Stupid elemental! Go back where you came from! I am the owner of this "
"estate and you will obey me!\""
msgstr "\"멍청한 정령아! 네가 왔던 곳으로 돌아가! 나는 이 부동산의 소유자이고 당신은 "
"나에게 복종 할 것입니다!\""

#. File = World/Cutscenes/Daeran_Q2_HeavenDoorstep/Daeran_Q2_CrateFireball/CommandBark 1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "9207174c-057f-485e-9b22-408d45b4faf7"
msgid ""
"\"Oh well, now I'm soaked to the bone! Give me a moment to change my "
"clothes, and don't forget to miss me while I'm gone!\""
msgstr "\"오, 이제 뼈까지 흠뻑 젖었어! 옷 갈아입을 시간 좀 주시고, 제가 없는 동안 "
"그리워하는 거 잊지 마세요!\""

#. File = World/Cutscenes/Daeran_Q2_HeavenDoorstep/Daeran_Q2_DaeranCamellia_Dance/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "e8875e7b-3598-429b-bef6-eaeee390f4e6"
msgid ""
"\"You are an extraordinary dancer, Lady Camellia. That is the truth, not "
"flattery.\""
msgstr "\"당신은 뛰어난 무용수입니다, 카멜리아 아가씨. 그것은 아첨이 아니라 "
"진실입니다.\""

#. File = World/Cutscenes/Daeran_Q2_HeavenDoorstep/Daeran_Q2_DaeranCamellia_Dance/CommandBark 1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "51283cda-9c68-4aba-8a4e-d859c4388683"
msgid ""
"\"You, on the other hand, are an extraordinary liar, Count, and that is the "
"truth, not an insult.\""
msgstr "\"반면에 당신은 특별한 거짓말쟁이입니다, 백작님, 그리고 그것은 모욕이 아니라 "
"진실입니다 \""

#. File = World/Cutscenes/Daeran_Q2_HeavenDoorstep/Daeran_Q2_DaeranSeelah_Dance/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "57857fdf-62bd-439b-b64c-0dfa13a37902"
msgid ""
"\"What's wrong, Count? Did you really expect me to do courtly waltzes?\""
msgstr "\"왜 그러세요, 백작님? 정말 제가 궁정 왈츠를 추길 기대하셨나요?\""

#. File = World/Cutscenes/Daeran_Q2_HeavenDoorstep/Daeran_Q2_DaeranSeelah_Dance/CommandBark 1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "f3f27af1-3b61-4f95-8d21-61a0218dc42e"
msgid "\"I don't feel like I'm the one leading in this dance...\""
msgstr "\"이 춤에서 내가 주도하는 것 같지 않아요...\""

#. File = World/Cutscenes/Daeran_Q2_HeavenDoorstep/Daeran_Q2_DaeranSeelah_Dance/CommandBark 2
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "cb8a024b-a8cf-4da4-883e-2a1286e8e4f1"
msgid ""
"\"Don't try to weasel out of it, now. You're not leaving this table until "
"the music is over!\""
msgstr "\"지금 당장 빠져나가려고 하지 마세요. 음악이 끝날 때까지 이 테이블을 떠나지 "
"마세요!\""

#. File = World/Cutscenes/Daeran_Q2_HeavenDoorstep/Daeran_Q2_DaeranSosiel_Talk/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "a11d3a98-bb0c-401d-843b-09accd5d96ea"
msgid ""
"\"Stop trying to talk me into this. You have neither heart nor conscience, "
"Daeran. Even the noble idea of charity becomes a farce when you touch it.\""
msgstr ""
"\"이 일에 대해 나를 설득하려고하지 마십시오. 넌 마음도 양심도 없어, 대런. "
"자선이라는 고귀한 생각조차도 당신이 그것을 만지면 희극이됩니다.\""

#. File = World/Cutscenes/Daeran_Q2_HeavenDoorstep/Daeran_Q2_DaeranSosiel_Talk/CommandBark 1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "21a8b05d-12c5-4b4b-8d2e-35fc7c7056d8"
msgid ""
"\"I don't think these so-called innocents will suffer from knowing that "
"their money and medicine came from my sinful silhouette.\""
msgstr ""
"\"나는 이 소위 무고한 사람들이 그들의 돈과 약이 내 죄 많은 실루엣에서 "
"나왔다는 것을 아는 것으로 고통받지 않을 것이라고 생각합니다 \""

#. File = World/Cutscenes/Daeran_Q2_HeavenDoorstep/Daeran_Q2_S3_GreyborNobleDialogue/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "bf4ede0e-2cc9-434b-8510-f7b971a77555"
msgid ""
"\"I'm sorry, sir, but I can't accept a job from a drunk client. You will "
"sober up tomorrow, regret your decision... and demand your money back.\""
msgstr ""
"\"죄송합니다만, 술에 취한 고객의 의뢰는 수락할 수 없습니다. 내일 술에서 깨고 "
"결정을 후회하고 돈을 돌려달라고 요구하세요.\""

#. File = World/Cutscenes/Daeran_Q2_HeavenDoorstep/Daeran_Q2_S3_GreyborNobleDialogue/CommandBark 1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "d804bd7a-aa4d-466a-874a-c88ab1dba993"
msgid ""
"\"N-n-no, I am ad… adamant! I need you to kill two... No, th-three "
"p-p-people!\""
msgstr "\"아니, 난 단호해.. 단호해! 둘을 죽여줘요.. 아니, 세 명을 죽여주세요!\""

#. File = World/Cutscenes/DefendersHeart/AlarmCutScene/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "1927c213-a0d4-47cf-82e9-b3843e8745dd"
msgid "\"Demons! The demons are attacking!\""
msgstr "\"데몬다! 데몬이 공격한다!\""

#. File = World/Cutscenes/DefendersHeart/AlarmCutScene/CommandBark 1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "abb9d0e5-e904-4f7f-acd8-0b24d6b484f0"
msgid ""
"\"Please, talk to Irabeth as soon as you can! Help us defend the tavern!\""
msgstr "\"제발, 최대한 빨리 이라베스와 얘기해 주세요! 선술집을 지킬 수 있도록 "
"도와주세요!\""

#. File = World/Cutscenes/DefendersHeart/DH_GemGolemMeet/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "1a8cc9dd-784f-4960-a6b2-b6c8b5028605"
msgid "\"Commander! Look what we've got here!\""
msgstr "\"사령관님! 여기 뭐가 있는지 보세요!\""

#. File = World/Cutscenes/DefendersHeart/DH_GemGolemMeet/CommandBark1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "40a3b414-2530-431d-bafe-8641753203c7"
msgid ""
"\"Are you two mad? Whatever possessed you to steal the gem golem from the "
"Tower of Estrod museum? Explain yourselves!\""
msgstr "\"너희 둘 미쳤어? 대체 무슨 생각으로 에스트로드의 탑 박물관에서 보석 골렘을 "
"훔친 거야? 설명해 봐!\""

#. File = World/Cutscenes/DefendersHeart/DH_GemGolemMeet/CommandBark2
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "151bc52c-df6b-40fa-85ec-fc76e29b8e4f"
msgid ""
"\"We didn't steal nothing, Commander! We saw some looters dragging it down "
"the street and recaptured it! Now, this shiny fella is our trophy by rights."
"\""
msgstr ""
"\"우린 아무것도 훔친 게 없습니다, 사령관님! 약탈자들이 길거리로 끌고 가는 걸 "
"보고 되찾아왔을 뿐입니다! 이제 이 반짝이는 친구는 당연히 우리의 전리품입니다."
"\""

#. File = World/Cutscenes/DefendersHeart/DH_GemGolemMeet/CommandBark3
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "27c041a8-1586-4e0d-b7e6-93b8f2f69131"
msgid ""
"\"That 'shiny fella' belongs to the treasury. The inlay alone is worth more "
"than a castle! Oh, I really don't like the idea of leaving this thing "
"unattended.\""
msgstr ""
"\"저 '반짝이는 친구'는 재무부에 속해 있습니다. 상감 세공만으로도 성보다 더 "
"가치가 있어요! 이걸 방치하는 건 정말 싫어요.\""

#. File = World/Cutscenes/DefendersHeart/DH_GemGolemMeet/CommandBark4
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "7ceed3f7-fff6-4246-8770-d1b34a6e653b"
msgid ""
"\"Joran! See to it that no one lays so much as a finger on this golem or "
"plucks out a single gem. And the same goes for you two. Your new mission is "
"to guard this 'shiny fella,' as you called it.\""
msgstr ""
"\"조란! 아무도 이 골렘에 손가락 하나라도 대거나 보석을 하나도 빼내지 "
"못하도록 조심해. 너희 둘도 마찬가지야. 너희의 새로운 임무는 이 '빛나는 "
"친구'를 지키는 거야.\""

#. File = World/Cutscenes/DefendersHeart/DH_StartDemonAttack_Pack2/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "777c009f-98d3-4b86-ab6e-bb4682d5c906"
msgid "\"It's over, mortals! Your feeble resistance is at an end!\""
msgstr "\"끝났다, 필멸자들아! 너희의 미약한 저항은 끝났다!\""

#. File = World/Cutscenes/DefendersHeart/DH_StartDemonAttack_Pack2/CommandBark 1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "ceba71de-870c-4643-9aa8-ceb0ba1b67ea"
msgid "\"There you are, Kilas. You won't escape this time.\""
msgstr "\"여기 있었구나, 킬라스. 이번엔 도망치지 못할 거야.\""

#. File = World/Cutscenes/DefendersHeart/DH_StartDemonsAttack/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "32992758-a699-46df-8623-fc4d790e46a0"
msgid "\"Kill the arsonists!\""
msgstr "\"방화범을 죽여라!\""

#. File = World/Cutscenes/DefendersHeart/StauntonVsCrusadersDH/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "8bcb3aac-35de-4e67-ab0b-bd53bc9376a5"
msgid ""
"\"Hey, Staunton! There's filth on the floor! Be a good lad and clean it up!\""
msgstr "\"이봐요, 스턴튼! 바닥에 오물이 있잖아! 착하게 굴어서 치워!\""

#. File = World/Cutscenes/DefendersHeart/StauntonVsCrusadersDH/CommandBark 1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "a6b675ab-d21e-4ebd-a910-c3b56abfad94"
msgid "\"Yes, sir. Right away.\""
msgstr "\"네, 선생님. 알겠습니다.\""

#. File = World/Cutscenes/DefendersHeart/StauntonVsCrusadersDH/CommandBark 2
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "230442e9-1d91-4743-8e11-65d556ba78ef"
msgid ""
"\"The sight of that traitorous dog turns my stomach! Why did the queen spare "
"his life?\""
msgstr "\"그 배신자 개를 보니 속이 뒤집혀요! 왜 여왕이 저 개를 살려줬을까?\""

#. File = World/Cutscenes/DefendersHeart/StauntonVsCrusadersDH/CommandBark 3
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "05e07bec-3179-4695-98a1-b98d1d0c61ff"
msgid "\"She should have strung him up, as a warning to all cultists!\""
msgstr "\"모든 이교도들에게 경고하는 의미로 그를 목매달았어야 했어요!\""

#. File = World/Cutscenes/DefendersHeart/StauntonVsCrusadersDH/CommandBark 4
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "52182fbb-9d1b-44cd-a16e-9596dcd5d088"
msgid "\"That she should have, sir.\""
msgstr "\"그래야만 했습니다.\""

#. File = World/Cutscenes/DefendersHeart/WoljifFirstScene_Basement/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "3b6d54ff-9d6f-4dad-a4e4-860787ab5dcb"
msgid "\"Surrender thy soul, Delvan!!!\""
msgstr "\"네 영혼을 항복하라, 델반!!!\""

#. File = World/Cutscenes/DefendersHeart/WoljifFirstScene_Basement/CommandBark 1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "192e44cf-6565-4e65-91c3-1caf9008a471"
msgid "\"Iomedae, protect us!\""
msgstr "\"아이오메데, 우릴 지켜줘!\""

#. File = World/Cutscenes/DefendersHeart/WoljifFirstScene_Basement/CommandBark 2
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "ae51e02f-3de7-474f-a01d-600cf7de8636"
msgid "\"Ha ha ha, got you again! How many times is that today?\""
msgstr "\"하하하, 또 잡혔네! 오늘 몇 번째야?\""

#. File = World/Cutscenes/DefendersHeart/WoljifFirstScene_Basement/CommandBark 3
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "7114df0f-73a8-42b8-bf5a-dee334d75558"
msgid "\"Take your jokes and shove 'em, tiefling!\""
msgstr "\"농담은 집어치워, 티플링!\""

#. File = World/Cutscenes/DefendersHeart/WoljifFirstScene_Basement/CommandBark 4
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "a9405917-58b6-4cf3-ba8b-22fccd00aa12"
msgid "\"Woah, easy there, chief! Don't hit me!\""
msgstr "\"와, 진정하세요, 대장님! 때리지 마세요!\""

#. File = World/Cutscenes/DragonsGraveyard/DragonsDialogue/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "c1d1e520-6e8b-4edf-bb0a-05a3b83b9a4d"
msgid ""
"\"You can't keep the alarm bell silenced forever, Hevelathimas. When it "
"rings, the fury of the metallic dragons will descend upon you! They'll "
"protect the sacred child!\""
msgstr ""
"\"당신은 알람 벨을 영원히 침묵시킬 수 없습니다, 헤벨라티마스. 이 종소리가 "
"울리면 금속 용의 분노가 당신에게 내려올 것입니다! 그들이 신성한 아이를 "
"지켜줄 거야!\""

#. File = World/Cutscenes/DragonsGraveyard/DragonsDialogue/CommandBark 1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "9ed7863a-5edf-449d-932f-37d299142d74"
msgid ""
"\"I don't need to silence the bell — I'm going to destroy it! But you won't "
"be alive long enough to witness its destruction!\""
msgstr ""
"\"나는 종을 침묵시킬 필요가 없다 - 나는 그것을 파괴할 것이다! 하지만 당신은 "
"그 파괴를 목격 할만큼 오래 살지 못할 것입니다!\""

#. File = World/Cutscenes/DrezenCapital/AeonQ3DayCutscene/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "97950611-c1b0-4308-a9bd-105edeaa462b"
msgid ""
"\"My friend, you know we don't make idle threats. If we say we're gonna do "
"it, we're gonna do it.\""
msgstr "\"이봐요, 우리는 헛된 협박을 하지 않아요. 우리가 하겠다고 하면 반드시 하죠.\""

#. File = World/Cutscenes/DrezenCapital/AeonQ3DayCutscene/CommandBark 1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "b2390fe2-b360-403f-bd7e-5a8c997eddf6"
msgid ""
"\"Oh! I recently heard a story about a merchant whose cart was set on fire "
"and then pitched off the top of a hill. Wanna hear it?\""
msgstr "\"아! 최근에 카트에 불이 나서 언덕 꼭대기에서 추락한 상인에 대한 이야기를 "
"들었습니다. 들어볼래요?\""

#. File = World/Cutscenes/DrezenCapital/AeonQ3DayCutscene/CommandBark 2
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "f2c3e558-4398-4e4a-ac5e-30c88f2613f6"
msgid "\"Just listen to us, and you'll be fine, my friend.\""
msgstr "\"우리 말만 잘 들으면 괜찮아질 거야, 친구.\""

#. File = World/Cutscenes/DrezenCapital/AeonQ3NightCutscene/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "c5a45f3e-d111-43b2-9b7f-9f9b92d5fca3"
msgid "\"For the last time, where is the money?\""
msgstr "\"마지막으로, 돈은 어디 있죠?\""

#. File = World/Cutscenes/DrezenCapital/AeonQ3NightCutscene/CommandBark 1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "12c1671a-594c-44ce-88c9-79fc0b1e8320"
msgid "\"Do I need to shake the money out of you? I'm tired of waiting!\""
msgstr "\"돈을 흔들어 줘야 하나요? 기다리는데 지쳤어요!\""

#. File = World/Cutscenes/DrezenCapital/AeonQ3NightCutscene/CommandBark 2
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "c935c7de-5d26-46a7-91a9-8f2bc40104c4"
msgid ""
"\"You do understand that if you don't give us what we want, there will be "
"others. See, right now, we're protecting you. But if we're gone, they'll pay "
"you a visit, and then what are you going to do?\""
msgstr ""
"\"우리가 원하는 것을 제공하지 않으면 다른 사람들이 있다는 것을 알고 "
"있습니다. 지금은 우리가 당신을 보호하고 있어요. 하지만 우리가 사라지면 "
"그들이 당신을 방문하게 될 텐데, 그때는 어떻게 할 건가요?\""

#. File = World/Cutscenes/DrezenCapital/AeonQ3NightCutscene/CommandBark 3
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "8056515e-4952-43fe-8f4c-aad8525be295"
msgid ""
"\"Get out! The Commander will find out about this, and then you'll be in "
"trouble!\""
msgstr "\"나가! 사령관이 이 사실을 알게 되면 큰일 날 거야!\""

#. File = World/Cutscenes/DrezenCapital/Azata_KTC_RobinHood/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "4258a665-5d95-4404-8089-7986430deedf"
msgid "\"I won't make it easy for you!\""
msgstr "\"내가 너를 쉽게 만들지 않을 거야!\""

#. File = World/Cutscenes/DrezenCapital/BedroomAssassination/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "e1853f89-cbda-4189-b6a0-9f236104de0a"
msgid "\"Where has {mf|he|she} gone?\""
msgstr "\"{mf|그|그녀} 어디 갔어?\""

#. File = World/Cutscenes/DrezenCapital/BedroomAssassination/CommandBark1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "18077bed-5041-4b5d-a7b0-41c32eddbad8"
msgid "\"{mf|He|She} couldn't sneak out of {mf|his|her} chambers past me.\""
msgstr "\"{mf|그|그녀}의 나를 지나 {mf|그|그녀}의 챔버에서 몰래 빠져나올 수 없습니다."
"\""

#. File = World/Cutscenes/DrezenCapital/BedroomAssassination/CommandBark2
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "9d0a960a-e101-42b7-a0c2-e477766032fe"
msgid "\"And here's the unwelcome intruder!\""
msgstr "\"그리고 여기 반갑지 않은 침입자가 있습니다!\""

#. File = World/Cutscenes/DrezenCapital/CamelliaQ1After_NewMurder/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "5bd7cb7c-bfa7-45a6-b20c-256a8d25fbc2"
msgid "\"Another murder? What's that commander playing at? Oh, it's you.\""
msgstr "\"또 살인? 저 사령관 뭐하는 거야? 오, 너구나 \""

#. File = World/Cutscenes/DrezenCapital/CamelliaQ1After_NewMurder/CommandBark 1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "c9ed44b0-8589-47ed-9125-32e567dcc739"
msgid ""
"\"Bad enough the demons are slaughtering us, now there's no peace even "
"inside Drezen.\""
msgstr "\"데몬들이 우리를 학살하는 것만으로도 충분히 나쁜데, 이제는 드레젠 "
"내부에서도 평화가 없습니다.\""

#. File = World/Cutscenes/DrezenCapital/CamelliaQ1After_NewMurder/CommandBark 2
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "1c53f979-ff42-4d62-aa09-7958d07b3af9"
msgid ""
"\"So somebody got killed, no big deal. I'll bet you that's what's happened "
"this time.\""
msgstr "\"그래서 누군가가 죽었어, 별거 아니야. 이번에도 그런 일이 일어났을 거라고 "
"장담합니다.\""

#. File = World/Cutscenes/DrezenCapital/CamelliaQ1_InBasement/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "d68a93ca-e6fa-4fc1-91c9-b5a064b45d3f"
msgid "\"Commander! What an unfortunate turn of events.\""
msgstr "\"사령관님! 이 얼마나 불행한 사건의 전환 \""

#. File = World/Cutscenes/DrezenCapital/CamelliaRomance_GoBalcony/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "1f289633-184a-4ece-948d-cdcffc6733ad"
msgid "\"Huh?\""
msgstr "\"허?\""

#. File = World/Cutscenes/DrezenCapital/CamelliaRomance_NoOneHere/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "420cd115-39db-4d39-8f40-ecaef751ed32"
msgid ""
"\"There's nobody here, you oaf! How would the enemy get in here — the very "
"heart of the crusader Citadel? Two extra stints on sentry duty for making "
"stupid jokes!\""
msgstr ""
"\"여기 아무도 없어, 이 멍청아! 적들이 십자군 성채의 심장부인 이곳에 어떻게 "
"들어올 수 있을까요? 멍청한 농담을 했다는 이유로 보초 근무를 두 번 더 "
"해야겠군!\""

#. File = World/Cutscenes/DrezenCapital/CamelliaRomance_NoOneHere/CommandBark 1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "74fcd36c-57ad-489f-8a13-e7d235ee9c98"
msgid "\"There's somebody here! I definitely heard something!\""
msgstr "\"여기 누군가 있어요! 분명히 뭔가 들었어요!\""

#. File = World/Cutscenes/DrezenCapital/CamelliaRomance_TempleSeek/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
#. 
#. File = World/Encounters/Legend_SoldiersParty/ActionsHolders/Legend_CrusadersBarks_Actions
#. Type = Kingmaker.ElementsSystem.ActionsHolder
#. Path = Actions/Actions[0]/IfTrue/Actions[0]/IfTrue/Actions[0]/Actions[1]/Action/Actions[0]/WhatToBark
msgctxt "13a9b300-24e7-4214-bd70-cba5e71d00ae"
msgid "\"Who's there?\""
msgstr "\"누구세요? \""

#. File = World/Cutscenes/DrezenCapital/CamelliaRomance_TempleSeek/CommandBark 1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "c98081d5-f75f-4249-a749-05efceb76377"
msgid "\"Was that a gust of wind?\""
msgstr "\"돌풍이었나요? \""

#. File = World/Cutscenes/DrezenCapital/CamelliaRomance_TempleSeek/CommandBark 2
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
#. 
#. File = World/Encounters/Legend_SoldiersParty/ActionsHolders/Legend_CrusadersBarks_Actions
#. Type = Kingmaker.ElementsSystem.ActionsHolder
#. Path = Actions/Actions[0]/IfTrue/Actions[0]/IfTrue/Actions[0]/Actions[0]/Action/Actions[0]/WhatToBark
msgctxt "c3ab03dd-01ca-4848-b785-893f6986d18e"
msgid "\"Huh? I thought I heard...\""
msgstr "\"어? 내가 들은 것 같은데...\""

#. File = World/Cutscenes/DrezenCapital/CamelliaRomance_TempleSexCaught/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "0b69a0c6-8892-4b56-8f41-538ee7bc8c8f"
msgid "\"Look! There, on the altar!\""
msgstr "\"저기 봐! 저기, 제단 위에!\""

#. File = World/Cutscenes/DrezenCapital/CamelliaRomance_TempleSexCaught/CommandBark 1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "ed349456-2ba3-4bd3-b87e-d384a28ea382"
msgid "\"It's the Commander! It's him and Camellia!\""
msgstr "\"사령관이다! 그와 카멜리아예요!\""

#. File = World/Cutscenes/DrezenCapital/CamelliaRomance_TempleSexCaught/CommandBark 2
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "542fe2b1-35c0-4f4c-8d66-1c5728b11d97"
msgid "\"I think we should leave now.\""
msgstr "\"지금 떠나야 할 것 같아요\""

#. File = World/Cutscenes/DrezenCapital/DoppelgangerExecuted/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "50f2eadc-1e24-4e62-961f-dfc09baa5cbc"
msgid "\"I know there cannot be two commanders in a crusader army.\""
msgstr "\"십자군 군대에는 두 명의 지휘관이 있을 수 없다는 것을 압니다.\""

#. File = World/Cutscenes/DrezenCapital/DoppelgangerFreed/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "a7732501-f942-489a-a68c-4f84fdf83ce1"
msgid "\"The way your prison chef cooks goat meat, mmm... It's something.\""
msgstr "\"교도소 요리사가 염소 고기를 요리하는 방식은.. 음.. 대단하죠.\""

#. File = World/Cutscenes/DrezenCapital/DoppelgangerInCapital/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "f6d21e80-90b0-46c8-b3f0-70a762e6bb41"
msgid ""
"\"We must strike swiftly and suddenly. There is no time to wait for "
"Galfrey's reinforcements. The army marches to Iz tonight!\""
msgstr "\"신속하고 갑작스럽게 공격해야 합니다. 갈프레이의 지원군을 기다릴 시간이 "
"없다. 군대는 오늘 밤 이즈로 진군한다!\""

#. File = World/Cutscenes/DrezenCapital/DoppelgangerInCapital/CommandBark 1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "30c82aab-73fb-4353-ac57-3329a3f6a301"
msgid "\"As you wish, {name}. We will do what you think is necessary.\""
msgstr "\"원하시는 대로 {name}. 필요하다고 생각하시는 대로 하겠습니다.\""

#. File = World/Cutscenes/DrezenCapital/DoppelgangerInCapital/CommandBark 2
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "70295a69-3aaa-4add-b100-3bec40017dce"
msgid ""
"\"Irabeth, your words sound an awful lot like treason. Are you sure you "
"don't want to take them back?\""
msgstr "\"이라베스, 당신의 말은 반역죄처럼 들립니다. 정말 취소하지 않으시겠어요?\""

#. File = World/Cutscenes/DrezenCapital/DoppelgangerInCapital/CommandBark 3
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "f4910fc5-ddcd-4a57-84d2-b34d64b887ee"
msgid ""
"\"I'm just worried, {name}. You've been acting strangely of late, and your "
"orders... are cause for some concern.\""
msgstr "\"그냥 걱정돼서 그래요, {name}. 최근에 이상하게 행동하고 있는데, 당신의 "
"명령이.. 걱정스러워요.\""

#. File = World/Cutscenes/DrezenCapital/DragonsCutscene1/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "92d9ea93-0cdc-495e-9ace-fc59c683de4b"
msgid ""
"\"Don't you get bored of living in the mountains for hundred of years? "
"Nothing ever happens up there.\""
msgstr "\"백 년 동안 산에서 사는 것이 지루하지 않습니까? 산 위에서는 아무 일도 "
"일어나지 않아요\""

#. File = World/Cutscenes/DrezenCapital/DragonsCutscene1/CommandBark1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "9b8ed584-0631-4b46-9c46-be76a65c652b"
msgid ""
"\"You humans are baffling. We have sunrises and sunsets... and those are "
"important events! After all, are there ever two identical days?\""
msgstr ""
"\"너희 인간들은 당황스럽다. 우리에게는 일출과 일몰이 있고.. 그것들은 중요한 "
"사건입니다! 결국, 같은 날이 두 번이나 있을 수 있나요?\""

#. File = World/Cutscenes/DrezenCapital/DragonsCutscene10/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "4bd9f284-f50e-422f-8391-787a0de3b54f"
msgid ""
"\"They say that Deskari cut off Terendelev's head with a single swing of his "
"scythe! I don't want that to happen to me!\""
msgstr "\"데스카리가 대낫을 한 번 휘둘러 테렌델레브의 목을 잘랐다고 해요! 나는 그런 "
"일이 일어나지 않기를 바랍니다!\""

#. File = World/Cutscenes/DrezenCapital/DragonsCutscene10/CommandBark1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "b9371a4c-158a-45f0-82ef-352c7a1b8c40"
msgid ""
"\"Then fight. But even if you do not wish to fight, you must still learn to "
"accept death with dignity. Even we dragons cannot escape its grasp forever.\""
msgstr ""
"\"그럼 싸워. 그러나 싸우고 싶지 않더라도 품위있게 죽음을 받아들이는 법을 "
"배워야합니다. 우리 용조차도 영원히 그 손아귀에서 벗어날 수 없습니다 \""

#. File = World/Cutscenes/DrezenCapital/DragonsCutscene11/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "0bc5fbc5-29d2-4a9c-ac56-c9479baa5037"
msgid ""
"\"Wait, I know you! You're Arvang! We used to be neighbors in Nerosyan. We "
"were childhood friends, remember? And then you moved somewhere...\""
msgstr ""
"\"잠깐만요, 당신을 알아요! 아르방이군요! 우린 네로시안에서 이웃이었어. 어릴 "
"적 친구였잖아, 기억나? 그리고 당신은 어딘가로 이사 갔어요.. \""

#. File = World/Cutscenes/DrezenCapital/DragonsCutscene11/CommandBark1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "bd5ef1a4-0bbd-4de6-a0a4-c5e69336917f"
msgid "\"I don't know what you're talking about, I've never seen you before.\""
msgstr "\"나는 당신이 무슨 말을 하는지 모르겠어요, 나는 당신을 본 적이 없어요.\""

#. File = World/Cutscenes/DrezenCapital/DragonsCutscene11/CommandBark2
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "01f50f8a-5013-437f-a06f-ad617c093fc0"
msgid ""
"\"I know I'm not mistaken. You don't want to admit it for some reason, but "
"it's the truth.\""
msgstr "\"나는 내가 착각하지 않았다는 것을 안다. 어떤 이유에서인지 인정하고 싶지 "
"않지만 진실입니다 \""

#. File = World/Cutscenes/DrezenCapital/DragonsCutscene11/CommandBark3
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "30a32578-14e0-4e21-a63d-50c7fe3b98a6"
msgid ""
"\"Arvang! Ha! You looked over at me when I called you, so that must be your "
"name! If your family is on the run, and you don't want to reveal yourself, "
"don't worry. I won't tell anyone that I saw you here.\""
msgstr ""
"\"아르방! 하! 내가 불렀을 때 날 쳐다봤으니 그게 네 이름일 거야! 가족이 도망 "
"중이고 자신을 드러내고 싶지 않더라도 걱정하지 마세요. 여기서 당신을 봤다고 "
"아무에게도 말하지 않을게요.\""

#. File = World/Cutscenes/DrezenCapital/DragonsCutscene11/CommandBark4
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "1450db1a-c437-4c75-9c49-a96b8f2fcd57"
msgid ""
"\"I'm not Arvang. I'm not in hiding, and no one is chasing me. If you don't "
"go away, human, I'll make you leave.\""
msgstr ""
"\"난 아르방이 아니에요. 나는 숨어 있지 않고 아무도 나를 쫓지 않습니다. "
"당신이 떠나지 않으면, 인간, 내가 당신을 떠나게 할 것입니다.\""

#. File = World/Cutscenes/DrezenCapital/DragonsCutscene11/CommandBark5
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "cd3b47de-aa83-4cb8-b0d8-ff0c8d8d4507"
msgid ""
"\"Did you just call me 'human'? That's odd. Whatever, I'll get to the bottom "
"of this!\""
msgstr "\"방금 저를 '인간'이라고 부르셨나요? 이상하네요. 어쨌든, 내가 알아낼게요!\""

#. File = World/Cutscenes/DrezenCapital/DragonsCutscene11/CommandBark6
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "f55540ef-8ac1-4e1a-9ab1-ffbb9804077c"
msgid ""
"\"Are you just going to run away every time you see me, Arvang? I couldn't "
"find out anything about your family, it's like they just vanished into thin "
"air, but...\""
msgstr "\"날 볼 때마다 그냥 도망칠 거야, 아르방? 네 가족에 대해 아무것도 알 수 "
"없었고, 그냥 허공으로 사라진 것 같지만...\""

#. File = World/Cutscenes/DrezenCapital/DragonsCutscene11/CommandBark7
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "de5b3476-b323-45f5-a681-d182417bd635"
msgid ""
"\"Enough, Raldo! If you want to know my secret — here it is. I come from a "
"family of gold dragons. We used to live in Nerosyan... but no one knew that "
"we were dragons. Yes, we used to be friends... so what? Can you accept that "
"I'm not human? I doubt it!\""
msgstr ""
"\"그만해, 랄도! 내 비밀을 알고 싶다면 여기 있습니다. 저는 골드 드래곤 가문 "
"출신이에요. 우린 네로시안에 살았지만.. 아무도 우리가 용이라는 사실을 "
"몰랐어요. 그래, 우린 친구였어.. 그래서 뭐? 내가 인간이 아니라는 걸 받아들일 "
"수 있겠어? 그럴 리가!\""

#. File = World/Cutscenes/DrezenCapital/DragonsCutscene11/CommandBark8
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "a3fc4823-73bc-4d79-8146-f0bf28e5601c"
msgid ""
"\"So you're... Well, I'll be damned! But I don't care if you're a dragon. "
"You're still the same Arvang... Well, almost the same. You're a bit taller "
"now!\""
msgstr ""
"\"그래서 당신은.. 젠장! 하지만 네가 용이라 해도 상관없어. 넌 여전히 똑같은 "
"아르방이야.. 거의 똑같아 키가 좀 더 커졌을 뿐이야\""

#. File = World/Cutscenes/DrezenCapital/DragonsCutscene2/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "af7190d7-a658-4043-830c-9bdb4b5a179e"
msgid "\"Is it true that dragons sleep in caves on top of treasure piles?\""
msgstr "\"용이 보물 더미 위에있는 동굴에서 잔다는 것이 사실입니까? \""

#. File = World/Cutscenes/DrezenCapital/DragonsCutscene2/CommandBark1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "63ae5d83-7eb6-4dd8-9d23-e7fe4441bbcb"
msgid "\"I prefer sleeping on soft fleece and leftover sheepskins.\""
msgstr "\"저는 부드러운 양털과 남은 양가죽에서 자는 것을 선호해요\""

#. File = World/Cutscenes/DrezenCapital/DragonsCutscene3/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "6303bd90-ceb2-49e8-8b65-80ca9377a716"
msgid ""
"\"You offer me your friendship, warm-blooded creature? Among all of the "
"bipeds, only elves deserve my friendship. The lifespans of the other races "
"are far too short to merit my attention.\""
msgstr ""
"\"우정을 베풀어주겠나, 온혈동물아? 모든 이족보행족 중에서 엘프만이 내 우정을 "
"받을 자격이 있다. 다른 종족의 수명은 내 관심을 끌기에는 너무 짧아.\""

#. File = World/Cutscenes/DrezenCapital/DragonsCutscene3/CommandBark1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "ce5b24f0-18ff-4dfc-b604-455432fef03d"
msgid ""
"\"You must not have many friends then... and I bet you're not very popular "
"with the other dragons either, are you?\""
msgstr "\"그럼 친구가 별로 없겠구나.. 다른 드래곤들에게도 인기가 없겠지? \""

#. File = World/Cutscenes/DrezenCapital/DragonsCutscene4/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "2cd6fd5d-41ec-4eb2-a8c2-f5e506b3a2da"
msgid ""
"\"Does it even matter to you if the Wound is closed? Don't you have your own "
"worries, like watching over the gold in your caves and all that?\""
msgstr "\"상처가 닫혀있는 것이 당신에게 중요합니까? 동굴의 금을 감시하는 것과 같은 "
"당신 자신의 걱정이 있지 않습니까? \""

#. File = World/Cutscenes/DrezenCapital/DragonsCutscene4/CommandBark1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "1221bd59-2239-4bd5-a50c-56a766be18d0"
msgid ""
"\"It matters very little to me whether or not the Wound is closed. But I "
"will do anything to alleviate the suffering of living beings.\""
msgstr "\"상처가 닫혔는지 여부는 저에게 거의 중요하지 않습니다. 그러나 나는 생명체의 "
"고통을 덜어주기 위해 무엇이든 할 것이다.\""

#. File = World/Cutscenes/DrezenCapital/DragonsCutscene5/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "5b8a0315-9639-4741-9e63-93a094fcae9c"
msgid ""
"\"Your lives are so fleeting. What's the point of wasting them on wars and "
"battles?\""
msgstr "\"너희들의 삶은 너무 덧없다. 전쟁과 전투에 낭비하는 것이 무슨 의미가 "
"있습니까?\""

#. File = World/Cutscenes/DrezenCapital/DragonsCutscene5/CommandBark1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "97e2d0be-6c40-434f-9d69-b6ec6b5533bf"
msgid ""
"\"Without the crusades, we would not survive for long. We're not fighting "
"because we enjoy it — we're defending that which is precious to us.\""
msgstr ""
"\"십자군 전쟁이 없었다면 우리는 오래 살아남지 못했을 것입니다. 우리는 그것을 "
"즐기기 위해 싸우는 것이 아니라 우리에게 소중한 것을 지키기 위해 싸우는 "
"것입니다.\""

#. File = World/Cutscenes/DrezenCapital/DragonsCutscene6/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "7eb6b01b-fd72-4a31-a76e-aca621c5abe3"
msgid ""
"\"This is not the kind of wasteland you are used to, young dragon. It only "
"brings death, so do not fly into it needlessly.\""
msgstr "\"여긴 너에게 익숙한 황무지가 아니란다, 어린 용아. 죽음을 가져올 뿐이니 "
"불필요하게 날아들지 마시오.\""

#. File = World/Cutscenes/DrezenCapital/DragonsCutscene6/CommandBark1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "41f2065b-f3d7-4f43-a18f-c401d946e285"
msgid ""
"\"I hate waiting around for orders. I want to fly! I need to spread my "
"wings! Why can't I fly out over the wasteland?\""
msgstr "\"나는 명령을 기다리는 것이 싫어요. 나는 날고 싶어요! 날개를 펼쳐야 해요! 왜 "
"황무지 위로 날아갈 수 없나요?\""

#. File = World/Cutscenes/DrezenCapital/DragonsCutscene7/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "184b3b14-0633-448c-b1c8-0de73ab84868"
msgid ""
"\"A young gold dragon — who isn't even a real dragon — has united all of us "
"in one place... Nothing like this has ever happened before!\""
msgstr "\"진짜 용도 아닌 어린 골드 드래곤이 우리 모두를 한곳에 모았습니다... 이런 "
"일은 전에 없었어요!\""

#. File = World/Cutscenes/DrezenCapital/DragonsCutscene7/CommandBark1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "6d71b0fb-e1d7-4dfc-ae2e-897239934fb9"
msgid ""
"\"Yet it is happening now... and we get to see it firsthand! This event will "
"go down in history, and we are here to witness it!\""
msgstr ""
"\"하지만 지금 일어나고 있습니다.. 그리고 우리는 그것을 직접 볼 수 있습니다! "
"이 사건은 역사에 기록될 것이며, 우리는 그것을 목격하기 위해 여기에 있습니다!"
"\""

#. File = World/Cutscenes/DrezenCapital/DragonsCutscene8/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "c51e2cee-aa1c-4a8c-b83d-1c85f6d54afc"
msgid ""
"\"Oh, yes! In my true form, I need to eat several sheep to satisfy my "
"hunger. As a human, all it takes is one chicken!\""
msgstr ""
"\"오, 그래! 내 진정한 형태에서는 배고픔을 채우기 위해 양을 여러 마리 "
"먹어야합니다. 인간으로서는 닭 한 마리만 먹으면 돼요!\""

#. File = World/Cutscenes/DrezenCapital/DragonsCutscene8/CommandBark1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "305a043f-ffda-4d2d-85bb-bb51e9c3e381"
msgid "\"This human form certainly has its advantages!\""
msgstr "\"이 인간 형태는 확실히 장점이 있습니다!\""

#. File = World/Cutscenes/DrezenCapital/DragonsCutscene9/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "5ef6318d-dd72-4d61-b805-32f6930069c8"
msgid ""
"\"What the demons did to Terendelev is unforgivable! She must be avenged!\""
msgstr "\"데몬들이 테렌델레브에게 한 짓은 용서할 수 없다! 그녀는 복수해야합니다!\""

#. File = World/Cutscenes/DrezenCapital/DragonsCutscene9/CommandBark1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "5a3976ed-05ab-459a-87d2-503fb6def713"
msgid ""
"\"That is a very dangerous and destructive way of thinking. Revenge only "
"leads to disappointment and misery. We are not here for revenge. We are here "
"to make sure other dragons do not share Terendelev's fate.\""
msgstr ""
"\"그것은 매우 위험하고 파괴적인 사고 방식입니다. 복수는 실망과 불행으로 "
"이어질 뿐입니다. 우리는 복수를 위해 여기 있지 않습니다. 우리는 다른 "
"드래곤들이 테렌델레브의 운명을 공유하지 않도록 하기 위해 여기에 있습니다.\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_AeonSecondState_CutSc1/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "88e29f64-d3ca-467a-80bc-881988da4abd"
msgid ""
"\"So what is the difference between golems and inevitables? You both look "
"the same to me.\""
msgstr "\"그래서 골렘과 필연적인 것의 차이점은 무엇입니까? 둘 다 똑같아 보여요\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_AeonSecondState_CutSc1/CommandBark1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "9df0fa0d-6db1-4a12-ac50-f9e994cf08bb"
msgid ""
"\"I possess a far more developed consciousness. Perhaps even more developed "
"than yours, human.\""
msgstr "\"나는 훨씬 더 발전된 의식을 가지고 있다. 아마도 당신보다 더 발달했을지도 "
"모르죠, 인간\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_AeonSecondState_CutSc10/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "76c9e18d-413d-4e98-b341-162e161dbe5e"
msgid ""
"\"This individual is unknown. The individual may state their classification."
"\""
msgstr "\"이 개체는 알 수 없습니다. 개인이 자신의 분류를 명시할 수 있습니다.\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_AeonSecondState_CutSc10/CommandBark1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "5e774071-5948-4ec3-bfb4-a5ebbb43ade7"
msgid ""
"\"My individual classification is Twelve-Nebula-Anomaly. This is my first "
"visit to Golarion.\""
msgstr "\"제 개인 분류는 12-성운-변칙성입니다. 골라리온 방문은 이번이 처음입니다.\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_AeonSecondState_CutSc10/CommandBark2
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "f809634d-78b6-456c-a56b-0e4ff0cbd3f7"
msgid ""
"\"This individual is classified as Ten-Negative Space. How does Twelve-"
"Nebula-Anomaly rate Golarion?\""
msgstr "\"이 개인은 열-음성 우주로 분류됩니다. 12-성운-변칙은 골라리온을 어떻게 "
"평가합니까?\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_AeonSecondState_CutSc10/CommandBark3
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "a8f50e9c-7288-4d53-a84c-fff12e8e7b48"
msgid ""
"\"Unsatisfactory. The level of chaos exceeds the levels acceptable in Axis. "
"The situation must be, and will be, corrected.\""
msgstr ""
"\"불만족스럽습니다. 혼돈의 수준이 Axis에서 허용되는 수준을 초과합니다. 이 "
"상황은 반드시 수정되어야 하며 수정될 것입니다.\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_AeonSecondState_CutSc11/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "437860f0-e95b-4b08-af6c-7bad49dcfe0b"
msgid ""
"\"This individual believes itself capable of deriving the formula that, once "
"implemented, will separate Golarion from the Worldwound\"."
msgstr "\"이 개인은 일단 구현되면 골라리온을 세계상처에서 분리할 수 있는 공식을 "
"도출할 수 있다고 믿는다\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_AeonSecondState_CutSc11/CommandBark1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "8e11677f-52ca-46d8-8443-9449c7ab13ec"
msgid "\"This individual doubts the possibility of such calculations.\""
msgstr "\"이 개인은 그러한 계산의 가능성을 의심합니다 \""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_AeonSecondState_CutSc11/CommandBark2
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "bb10ded3-db2c-4a45-9e08-23905b2daa69"
msgid ""
"\"This individual does not believe its intellectual capability to be lower "
"than that of Areelu Vorlesh.\""
msgstr "\"이 개인은 자신의 지적 능력이 아릴루 볼레쉬보다 낮다고 생각하지 않습니다\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_AeonSecondState_CutSc11b/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "2d4628e1-dadb-43c0-8c6d-a9e2bea9058b"
msgid ""
"\"The individual before me has been neglecting the tasks required to "
"maintain its biological functions for several 24-hour periods now.\""
msgstr "\"내 앞에있는 개인은 현재 몇 시간 동안 생물학적 기능을 유지하는데 필요한 "
"작업을 소홀히하고 있습니다 \""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_AeonSecondState_CutSc11b/CommandBark1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "e19d69a4-272e-498f-b534-3a19b3b2566c"
msgid ""
"\"This individual fully controls its biological needs. It devotes its time "
"to research the Golarion and the Abyss separation formula.\""
msgstr "\"이 개체는 생물학적 필요를 완전히 통제합니다. 골라리온과 심연 분리 공식을 "
"연구하는데 시간을 할애합니다 \""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_AeonSecondState_CutSc11b/CommandBark2
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "54b458c8-0d69-4fe2-b187-c5eb12b4057c"
msgid ""
"\"The mirror neurons of this individual state the individual in front of it "
"is experiencing physiological distress. Error?\""
msgstr "\"이 개체의 거울 뉴런은 그 앞에있는 개체가 생리적 고통을 겪고 있다고 "
"말합니다. Error? \""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_AeonSecondState_CutSc11b/CommandBark3
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "16ffa4f1-7c99-4e59-b6a1-7db9886aedaa"
msgid "\"Error. There is no need to show concern for this individual.\""
msgstr "\"오류. 이 개인에 대한 우려를 표시할 필요가 없습니다.\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_AeonSecondState_CutSc11c/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "2a9553be-ada0-46f6-a683-12cc55b8c006"
msgid "\"m = 2.1 + 5 lgD! L = YT14^412b2!\""
msgstr "\"m = 2.1 + 5 lgD! L = YT14^412b2!\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_AeonSecondState_CutSc11c/CommandBark1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "17232029-41ea-442c-910f-e7f2fce18119"
msgid ""
"\"This individual's mind does not perceive the connection between these two "
"formulas.\""
msgstr "\"이 개인의 정신은 이 두 공식 사이의 연관성을 인식하지 못합니다\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_AeonSecondState_CutSc11c/CommandBark2
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "cd081643-1c26-4f19-a6e1-000d23466a62"
msgid "\"T= ab2! AHAHAHAHA! T= ab2! T= ab2! HA! HA! HA! T = ab2!\""
msgstr "\"T= ab2! AHAHAHAHA! T= ab2! T= ab2! HA! HA! HA! T = ab2!\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_AeonSecondState_CutSc11c/CommandBark3
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "914d248b-d07a-4116-9983-ac478453f443"
msgid ""
"\"The mind of the individual before me has been irreversibly damaged due to "
"circumstances outside of its control. The individual will be provided with "
"necessary assistance.\""
msgstr ""
"\"내 앞에 있는 개인의 정신은 통제할 수 없는 상황으로 인해 돌이킬 수 없을 "
"정도로 손상되었습니다. 개인에게 필요한 지원이 제공될 것입니다.\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_AeonSecondState_CutSc2/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "541ef560-e0e8-4b87-83f9-7eba86d57e14"
msgid ""
"\"Mortal flesh is easily damaged. Yet you still go into battle. This is "
"inefficient. Why do you not make golems to fight in your stead?\""
msgstr ""
"\"필멸자의 육체는 쉽게 손상됩니다. 하지만 당신은 여전히 전투에 참여합니다. "
"이것은 비효율적입니다. 왜 당신을 대신하여 싸울 골렘을 만들지 않습니까?\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_AeonSecondState_CutSc2/CommandBark1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "e38ebb32-0b41-4a35-a732-5304b898bda3"
msgid ""
"\"Golems are more expensive than people. To build a golem you need spells, "
"materials, workshops... But people make new people for free. Golems are "
"works of art, and we're expendable material at best.\""
msgstr ""
"\"골렘은 사람보다 비싸다. 골렘을 만들려면 주문, 재료, 작업장이 필요합니다... "
"하지만 사람들은 무료로 새로운 사람을 만듭니다. 골렘은 예술 작품이고 우리는 "
"기껏해야 소모품입니다.\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_AeonSecondState_CutSc3/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "0331007b-e70b-48fe-a568-820fc930c35b"
msgid ""
"\"So what's it like on Axis? It's probably better than this gods-forsaken "
"Worldwound!\""
msgstr "\"그래서 엑시스에서는 어때요? 이 신이 버린 세계상처보다는 낫겠지!\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_AeonSecondState_CutSc3/CommandBark1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "2b54b113-cefc-4965-ba52-e168114d37da"
msgid ""
"\"I do not make assessments. I complete tasks. My task is to fight for the "
"Commander.\""
msgstr "\"나는 평가를 하지 않는다. 나는 임무를 완수한다. 내 임무는 사령관을 위해 "
"싸우는 것입니다.\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_AeonSecondState_CutSc4/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "00a2af39-4704-43e0-a22e-4b115e40f8e3"
msgid ""
"\"Understanding our goal gives us an advantage in battle. Not all human "
"combat units possess this ability.\""
msgstr "\"우리의 목표를 이해하면 전투에서 유리합니다. 모든 인간 전투 유닛이 이 "
"능력을 가지고 있는 것은 아닙니다 .\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_AeonSecondState_CutSc4/CommandBark1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "f0dc881f-78e0-49c1-89e0-b38163a38a59"
msgid ""
"\"I do not understand. Does not every unit here possess the goal of fighting "
"for the Commander?\""
msgstr "\"이해가 안 돼요. 여기있는 모든 부대는 사령관을 위해 싸우는 목표를 가지고 "
"있지 않습니까?\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_AeonSecondState_CutSc5/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "fe4d7b25-d3f4-40a2-9dc4-fac2e07e2983"
msgid ""
"\"Combat unit, your service to the aeon has been assessed as effective. Your "
"success will be noted.\""
msgstr "\"전투 부대, 당신의 에온에 대한 봉사가 효과적인 것으로 평가 되었습니다. "
"당신의 성공은 주목할 것입니다\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_AeonSecondState_CutSc5/CommandBark1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "0a626a76-b3a3-4c97-b731-0860de8763d3"
msgid "\"I seek to achieve optimal effectiveness.\""
msgstr "\"최적의 효과를 얻기 위해 노력합니다\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_AeonSecondState_CutSc6/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "49c0a1c9-fda1-4204-b661-072a5bc94d25"
msgid ""
"\"The effectiveness of this solution is questionable. The inevitables must "
"fight the enemies of Axis. This individual does not consider the coefficient "
"of their utility in Drezen satisfactory.\""
msgstr ""
"\"이 솔루션의 효과는 의심 스럽습니다. 피할 수 없는 사람은 축의 적과 싸워야 "
"합니다. 이 개인은 드레젠의 효용 계수를 만족스럽게 생각하지 않습니다.\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_AeonSecondState_CutSc6/CommandBark1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "799ecf86-0968-4871-86ee-d3bae67166da"
msgid ""
"\"The individual before me has made a mistake in its calculation. The "
"effectiveness of this solution is calculated using the following formula...\""
msgstr "\"내 앞에있는 개인이 계산에 실수를했습니다. 이 솔루션의 효과는 다음 공식을 "
"사용하여 계산됩니다...\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_AeonSecondState_CutSc7/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "0aee6002-a1c7-4d02-8837-d9122dfc0126"
msgid "\"Initiate combat skill test.\""
msgstr "\"전투 기술 테스트 시작\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_AeonSecondState_CutSc7/CommandBark1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "19f13b0f-7747-4fd4-b405-d49c12e5ec67"
msgid "\"Combat skill test initiated.\""
msgstr "\"전투 기술 테스트 시작되었습니다.\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_AeonSecondState_CutSc7/CommandBark2
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "ff0df225-9d3d-48c5-81e5-7a78965021b3"
msgid "\"Satisfactory. Proceed.\""
msgstr "\"만족. Proceed.\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_AeonSecondState_CutSc8/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "6ac4383a-f909-4935-8d86-7d444320eb6a"
msgid ""
"\"Areelu Vorlesh must be destroyed at all costs. Her existence will be "
"terminated by any means necessary.\""
msgstr "\"아릴루 볼레쉬는 어떤 대가를 치르더라도 파괴해야 합니다. 그녀의 존재는 "
"필요한 모든 수단으로 종결 될 것입니다 \""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_AeonSecondState_CutSc8/CommandBark1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "03e00482-eacc-4184-9e58-e06b7418f240"
msgid ""
"\"Note: Areelu Vorlesh's achievements must be catalogued and studied. "
"Destroying the results of scientific progress is irrational.\""
msgstr "\"참고: 아릴루 볼레쉬의 업적은 반드시 목록화하여 연구해야 합니다. 과학적 "
"진보의 결과를 파괴하는 것은 비합리적입니다.\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_AeonSecondState_CutSc9/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "7322dda4-8a18-4bdc-ada5-be5459a77732"
msgid ""
"\"This individual is authorized to conduct research on the level of "
"discomfort axiomites experience in the location colloquially known as the "
"\"Worldwound\". Rate your condition on a scale from one to ten, where ten is "
"equal to unbearable suffering.\""
msgstr ""
"\"이 개인은 구어체로 \"세계상처\"로 알려진 위치에서 공리주의자들이 경험하는 "
"불편함의 정도에 대한 연구를 수행할 권한이 있습니다. 1에서 10까지의 척도로 "
"귀하의 상태를 평가하십시오(10은 참을 수 없는 고통과 동일).\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_AeonSecondState_CutSc9/CommandBark1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "fd64ea30-64ff-409c-8877-9e5e0952b57f"
msgid "\"Five.\""
msgstr "\"다섯.\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_AeonSecondState_CutSc9/CommandBark2
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "80b66ce5-4c69-4ef1-9860-4326f128c2fc"
msgid ""
"\"The provided value falls within the recommended range. The individual is "
"hereby granted permission to remain in Drezen.\""
msgstr "\"제공된 값이 권장 범위 내에 있습니다. 이에 따라 개인은 드레젠에 머무를 수 "
"있는 권한이 부여됩니다\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_DemonAltar_CutSc/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "d4c89548-6da2-4184-b5b9-49e640cf7b00"
msgid ""
"\"Stab him. Stab him for the glory of your new dark masters. Or do you want "
"to take his place on the altar?\""
msgstr "\"그를 찌르세요. 새로운 어둠의 주인의 영광을 위해 그를 찌르십시오. 아니면 "
"제단에서 그의 자리를 차지하고 싶습니까?\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_DemonAltar_CutSc/CommandBark1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "24235eed-2e0e-48f4-ad61-4703ea5b1b9c"
msgid ""
"\"No! Please, I don't want to! It's just... He's my comrade! My brother-in-"
"arms!\""
msgstr "\"아니요! 제발요, 그러고 싶지 않아요! 그냥.. 그는 내 동지예요! 내 전우라고요!"
"\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_DemonAltar_CutSc/CommandBark2
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "7c5c9208-19b7-4328-bdee-1872e0d9e615"
msgid "\"Please, don't do this! Don't listen to the demons!\""
msgstr "\"제발, 이러지 마세요! 데몬들 말 듣지 마세요!\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_DemonAltar_CutSc/CommandBark3
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "71c7aadf-e42f-4a0c-95fd-620cba26150d"
msgid "\"Yes, just give me a moment...\""
msgstr "\"네, 잠시만요...\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_DemonAltar_CutSc/CommandBark4
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "a83816df-c6aa-4351-ac30-af7acad91898"
msgid ""
"\"Don't worry! Iomedae won't punish you. We're your new masters. We're the "
"ones in charge now. So grab the knife, and stab him!\""
msgstr ""
"\"걱정 마세요! 아이오메데는 널 벌하지 않을 거야. 우리가 너의 새로운 "
"주인이다. 이제 우리가 주인이다. 그러니 칼을 들고 그를 찌르세요!\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_DemonSlaveMarket_CutSc/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "4183dd3d-e605-47b9-936b-ad1d4eb5696a"
msgid ""
"\"Mortals for sale, mortals of the highest quality! Former crusaders! A fine "
"selection of cultists! Affordable prices!\""
msgstr "\"필멸자 판매, 최고 품질의 필멸자! 전직 십자군! 엄선된 이교도들! 저렴한 가격!"
"\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_DemonSlaveMarket_CutSc/CommandBark1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "d6a176ab-ffa1-474f-9d9d-fd6c93b78e89"
msgid ""
"\"All right, mortal scum. I need a personal servant. Which one of you will "
"be the most loyal and obedient?\""
msgstr "\"좋아, 필멸의 쓰레기들아. 나는 개인 하인이 필요하다. 너희 중 누가 가장 "
"충성스럽고 순종적일까?\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_DemonSlaveMarket_CutSc/CommandBark2
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "5226993a-3053-48ab-82f4-6220635cc146"
msgid "\"Me! Pick me! I'll serve you well!\""
msgstr "\"저요! 저를 골라주세요! 잘 모시겠습니다!\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_DemonSlaveMarket_CutSc/CommandBark3
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "dbee0340-e87d-4bb4-9124-1683188dfbef"
msgid "\"No, I can tell that you used to be a priest. I want someone else.\""
msgstr "\"아니요, 전에는 사제였던 것 같아요. 다른 사람을 원해요\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_Demon_CultistDialog/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "23f8636c-6a24-4ac5-ade2-4fe18a8f33f5"
msgid ""
"\"When the demons aren't off fighting their enemies, they're here "
"slaughtering their allies!\""
msgstr "\"데몬들이 적과 싸우지 않을 때, 그들은 여기 아군을 학살하고 있습니다!\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_Demon_CultistDialog/CommandBark1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "11445d51-0293-4711-8142-d7a1589a402b"
msgid ""
"\"Yes, a couple of my friends were killed not too far from here. The demons "
"tore them to pieces... just for fun!\""
msgstr "\"네, 제 친구 두 명이 여기서 멀지 않은 곳에서 살해당했습니다. 데몬들이 "
"그들을 갈기갈기 찢어 버렸어요.. 그냥 재미로!\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_Demon_CultistDialog/CommandBark2
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "73577bba-6afa-43da-9693-9021f7970cdd"
msgid ""
"\"Why should the Commander worry about mortal servants? If {mf|he|she} runs "
"out of them — {mf|he|she} will just conquer some more.\""
msgstr ""
"\"사령관이 왜 필멸자 종들을 걱정해야 하나요? 만약 {mf|그|그녀}의 다 떨어지면 "
"{mf|그|그녀}의 더 정복할 것입니다.\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_Demon`sPrisoners_CutSc/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "8f98fa4d-5793-4275-8335-3eb4987560ba"
msgid ""
"\"I'll offer you the same deal as all the other prisoners. There are six of "
"you, and we only have two positions available in our entourage.\""
msgstr "\"다른 모든 죄수들과 똑같은 거래를 제안하겠습니다. 당신들은 여섯 명이고, "
"우리 측근에는 두 자리밖에 없습니다\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_Demon`sPrisoners_CutSc/CommandBark1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "e19f9ff2-c5ec-4faa-b14e-bb5c85bc6871"
msgid ""
"\"We'll give you knives, and you can decide among yourselves who will become "
"our servants. Any objections?\""
msgstr "\"우리는 당신에게 칼을 줄 것이고, 누가 우리의 하인이 될지 당신들끼리 결정할 "
"수 있습니다. 이의 있습니까?\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_Demon`sPrisoners_CutSc/CommandBark2
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "b7c51a6f-1459-40a0-a927-ffc47b805f7e"
msgid "\"My lord will punish you!\""
msgstr "\"내 주님께서 당신을 벌하실 것입니다!\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_Demon`sPrisoners_CutSc/CommandBark3
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "54ae2678-a1dd-4525-b70f-bc5c38ace6a2"
msgid ""
"\"Hm... Well, if he is so powerful, then I won't bother giving you a knife. "
"I'm sure he can help you win the fight.\""
msgstr ""
"\"흠.. 그가 그렇게 강력하다면, 나는 당신에게 칼을 주는 것을 귀찮게하지 않을 "
"것입니다. 그가 당신이 싸움에서 이길 수 있도록 도와 줄 수 있다고 확신합니다.\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_DevilState_2_CutSc/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "12f04784-6b93-45f4-ad05-dc4a7d421584"
msgid ""
"\"I never thought I'd thank a devil, but if not for that contract of yours, "
"I'd be worm food! Now I feel I can fight the demons on equal terms!\""
msgstr ""
"\"데빌에게 감사할 줄은 몰랐지만, 당신의 계약이 아니었다면 나는 벌레의 먹이가 "
"되었을 것입니다! 이제 데몬들과 동등한 조건으로 싸울 수 있을 것 같아요!\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_DevilState_2_CutSc/CommandBark1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "0b0a1cd5-ca33-4207-9aa4-98698800aa1a"
msgid ""
"\"Make sure to tell your friends about me. They won't regret it. You'll see."
"\""
msgstr "\"친구들에게 나에 대해 꼭 말해줘. 그들은 후회하지 않을 것입니다. 알게 될 "
"거예요\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_DevilState_4_CutSc/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "0c2df807-441d-44b0-ba1e-768c49c96629"
msgid ""
"\"Is it prudent to leave Hell for such a long time? What if I lose my "
"position?\""
msgstr "\"그렇게 오랫동안 지옥을 떠나는 것이 현명한가요? 지위를 잃으면 어떻게 되나요?"
"\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_DevilState_4_CutSc/CommandBark1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "e6936cb6-8a6d-44ea-ae09-4d09075e2e14"
msgid ""
"\"Ha! We'll be the ones to close the Worldwound, that vile chaos-born rift! "
"This will bring us more honors than you could ever imagine!\""
msgstr "\"하! 혼돈이 낳은 사악한 균열, 세계상처를 닫는 건 우리가 될 거야! 너희가 "
"상상할 수 없는 영광을 가져다줄 거야!\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_DevilState_8_CutSc/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "037e0ce4-a171-4cf1-ab2a-d85e88b5fdf9"
msgid ""
"\"I used to think you should stay away from devils... but seeing how "
"fiercely they fight, I find my hand reaching for my sword. With this lot on "
"our side, victory will be ours!\""
msgstr ""
"\"예전에는 데빌들을 멀리해야 한다고 생각했지만.. 그들이 얼마나 치열하게 "
"싸우는지 보니 내 손이 검에 닿는 것을 발견했습니다. 이 많은 사람들이 우리 "
"편이라면 승리는 우리의 것이 될 것입니다!\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_DevilState_8_CutSc/CommandBark1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "27a59322-d86b-4348-9aec-96025999a17a"
msgid ""
"\"People live well in Cheliax. Why shouldn't we? Strength, power, and order —"
" that's how we'll win!\""
msgstr "\"첼리악스에서는 사람들이 잘 살고 있어요. 왜 안 될까요? 힘, 힘, 질서 - "
"그것이 우리가 이기는 방법입니다!\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_TavernEasterEgg_FF/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "ddff980e-0cb9-447f-8e41-755870692250"
msgid ""
"\"A friend told me Queen Galfrey has the best horse in Mendev. Maybe even "
"the best in the world.\""
msgstr "\"친구가 갈프레이 여왕이 멘데브에서 최고의 말을 가지고 있다고 말했어요. 아마 "
"세계 최고일지도 모르죠.\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_TavernEasterEgg_FF/CommandBark1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "ad3d7546-3e0b-4148-822a-43a75dac8d6a"
msgid ""
"\"Here's what I think: it doesn't matter what's under the saddle. The only "
"thing that matters is who's holding the reins!\""
msgstr "\"제 생각은 이렇습니다. 안장 밑에 무엇이 있는지는 중요하지 않습니다. 중요한 "
"것은 누가 고삐를 잡고 있느냐는 것입니다!\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_TavernEasterEgg_FF/CommandBark2
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "a7d35925-bc5c-4a94-b957-123f5dd5a3e5"
msgid ""
"\"He said it's so fast and furious that she rides around Nerosyan, leaping "
"from one roof to another, never touching the streets!\""
msgstr ""
"\"그는 그것이 너무 빠르고 맹렬해서 네로시안을 돌아 다니며 지붕에서 다른 "
"지붕으로 뛰어 다니며 거리를 건드리지 않는다고 말했습니다!\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_TavernEasterEgg_GoT/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "1e4d3000-6d75-4a05-bfb4-b6d9dcd9b67b"
msgid ""
"\"Don't tell anyone, but I've got that book banned by the Mendevian "
"censors...\""
msgstr "\"아무한테도 말하지 마세요, 하지만 멘데비안 검열관이 금지한 책이 있어요...\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_TavernEasterEgg_GoT/CommandBark1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "891e2cac-9980-40fa-a2d1-f74c63dd9aa0"
msgid ""
"\"Yes, yes, 'The Game of Owns'! Don't be so loud, or I won't let you borrow "
"it.\""
msgstr "\"예, 예, '게임 오브 오너스'! 너무 시끄럽지 않으면 빌려주지 않을거야 \""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_TavernEasterEgg_GoT/CommandBark2
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "9e228bfc-c84d-43fd-b8ef-484857afe469"
msgid ""
"\"Really? The one where Baphomet and his vile family backstab each other, "
"indulge in debauchery, and do other outrageous things?\""
msgstr "\"정말? 바포메트와 그의 사악한 가족이 서로를 뒤통수 치고 방탕에 빠지고 다른 "
"터무니없는 일을 하는 곳?\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_TavernEasterEgg_JB/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "006fd321-4fc2-4612-ad84-aae630705385"
msgid ""
"\"...and then that traitor chained the Commander to the torture rack, turned "
"on the Ray of Slow Death, and laughed.\""
msgstr "\"...그리고 그 배신자는 사령관을 고문대에 묶고 느린 죽음의 광선을 켜고 "
"웃었다 \""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_TavernEasterEgg_JB/CommandBark1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "1dd535d5-9917-4003-b301-27f128b66191"
msgid ""
"\"'And now, Commander,' the traitor said, all traitor like, 'I expect you to "
"die!'\""
msgstr "\"'그리고 지금, 사령관, 반역자는 '나는 당신이 죽을 것으로 기대합니다!'\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_TavernEasterEgg_JB/CommandBark2
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "ecc65a96-dd1e-4dba-bf5f-30d868a8667e"
msgid "\"Incredible! But how did {mf|he|she} escape?\""
msgstr "\"놀랍습니다! 하지만 {mf|그|그녀}는 어떻게 탈출했나요?\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_TavernEasterEgg_JB/CommandBark3
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "aee9022c-8ccc-493e-a0c1-84331cf66eec"
msgid ""
"\"Wouldn't you want to know? Well, I can tell you. {mf|He|She} used a tiny "
"lockpick disguised as a fake tooth!\""
msgstr ""
"\"알고 싶지 않으세요? 글쎄요, 말씀드릴 수 있습니다. {mf|그|그녀}는 가짜 "
"이빨로 위장한 작은 자물쇠 집게를 사용했어요!\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_TavernEasterEgg_JB/CommandBark4
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "fa2a197d-276d-4e94-bdbc-8c8d888a76a8"
msgid "\"No way! The Commander has fake teeth?\""
msgstr "\"말도 안돼! 사령관이 가짜 이빨을 가지고 있다고?\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_TavernEasterEgg_Marvel/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "58e787bc-43ac-480e-afd1-7d11c6edf799"
msgid ""
"\"I say, there are too many freeloaders in the citadel. The Commander should "
"get rid of at least half, or else we'll soon have nothing to eat.\""
msgstr "\"성채에 프리 로더가 너무 많다고 말합니다. 사령관이 적어도 절반을 제거하지 "
"않으면 곧 먹을 것이 없을 것입니다.\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_TavernEasterEgg_Marvel/CommandBark1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "8cb6b9f6-7b6f-41b2-be70-3b147cc2d7ca"
msgid ""
"\"How? You can't just snap your fingers and make half the citadel's "
"population disappear!\""
msgstr "\"어떻게? 손가락만 까딱하면 성채 인구의 절반이 사라질 수는 없어!\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_TavernEasterEgg_PR/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "8e39bd39-b54c-4836-8b4b-e561c6f7a8e7"
msgid ""
"\"Word on the street is that soon we'll have some Azlanti artifacts on our "
"side.\""
msgstr "\"소문에 따르면 곧 아즐란트 유물이 우리 편이 될 거라고 합니다\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_TavernEasterEgg_PR/CommandBark1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "42dc3c9e-a01d-4f9e-b376-aaa054b78832"
msgid ""
"\"I heard that rumor! Project 'Demonic Grim,' right? But what exactly is it?"
"\""
msgstr "\"그 소문 들었어! 프로젝트 '데모닉 그림' 맞죠? 하지만 정확히 뭔데요?\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_TavernEasterEgg_PR/CommandBark2
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "5363ea9d-b1a8-4f2f-8efb-3d645a0fbd7d"
msgid ""
"\"They say it's giant metal golems, each taller than the citadel. They'll "
"squish that giant bug Deskari in no time!\""
msgstr "\"그들은 그것이 성채보다 큰 거대한 금속 골렘이라고 말합니다. 거대한 벌레 "
"데스카리를 순식간에 뭉개버릴 거라고요!\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_Trickster_CutScBook_3/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "1a339f8b-a234-4987-b82d-ba2b25c8064a"
msgid "\"Hahaha! Oh, this is too funny! Hilarious!\""
msgstr "\"하하하! 오, 너무 웃겨요! 웃겨요!\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_Trickster_CutScBook_3/CommandBark1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "3d021985-129f-42aa-99de-7d7c309483e2"
msgid "\"What did it say?\""
msgstr "\"뭐라고 했어? \""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_Trickster_CutScBook_3/CommandBark2
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "af607529-21bc-4a80-8d36-e7f96c30f0e6"
msgid "\"Beats me!\""
msgstr "\"나를 이겼어!\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_Trickster_CutScBook_4/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "b24d0b17-9f52-4d45-8961-269f3e2b2837"
msgid "\"Silly hats! Who wants a silly hat?\""
msgstr "\"바보 모자! 누가 바보 모자 원해?\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_Trickster_CutScBook_4/CommandBark1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "d62deb73-0059-45b6-b55c-45dba11fcee2"
msgid "\"Can I have one?\""
msgstr "\"하나 가져도 돼요? \""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_Trickster_CutScBook_4/CommandBark2
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "e60c2f68-6663-4d47-99dd-7df4bea38b0e"
msgid "\"No! You are not allowed!\""
msgstr "\"아니요! 당신은 허용되지 않습니다!\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_Trickster_CutScBook_5/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "5cfd13f2-cdf2-438a-8df4-038f8ef836b2"
msgid "\"Excuse me, how do I get to everywhere?\""
msgstr "\"실례합니다, 어떻게 모든 곳으로 갈 수 있습니까? \""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_Trickster_CutScBook_5/CommandBark1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "29736e69-9686-4509-a1fc-32aa5780ac1c"
msgid ""
"\"It's straight ahead, then back. Once you're at the crossroads — stop there "
"and spin around on the spot.\""
msgstr "\"직진했다가 돌아서면 됩니다. 교차로에 도착하면 거기서 멈추고 그 자리에서 "
"빙글빙글 돌아요\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_Trickster_CutScBook_5/CommandBark2
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "d9b83710-f403-4e7d-b51b-7357e73c780c"
msgid "\"And then?\""
msgstr "\"그리고 나서? \""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_Trickster_CutScBook_5/CommandBark3
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "a6317447-5192-44cf-b89e-aa113184bfdd"
msgid "\"And then you're everywhere!\""
msgstr "\"그리고 당신은 어디에나 있습니다!\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_Trickster_CutScDog_2/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "12908d26-8944-4204-9d25-f9ae55e22d22"
msgid "\"Who's a good boy?\""
msgstr "\"누가 착하지? \""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_Trickster_CutScDog_2/CommandBark1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "80cb5d75-bd6d-4016-a15b-bee18694b3fd"
msgid "\"That would be me.\""
msgstr "\"그건 나야\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_Trickster_CutScDog_2/CommandBark2
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "1fd8b507-ad10-4c60-819f-f1ed385f8dd0"
msgid "\"Who's a sweetie-pie?\""
msgstr "\"누구야?\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_Trickster_CutScDog_2/CommandBark3
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "f85e82e2-4f43-4c28-a530-5318b17c35c5"
msgid "\"Me again.\""
msgstr "\"또 나\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_Trickster_CutScDog_2/CommandBark4
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "b779e3b7-ae97-4bdd-8a68-f27271a4c32b"
msgid "\"Who's a silly little goofball?\""
msgstr "\"누가 멍청한 바보야?\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_Trickster_CutScDog_2/CommandBark5
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "7db0910b-d65b-4863-ab16-4029e909e3d2"
msgid "\"That's you.\""
msgstr "\"그건 너야\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_Trickster_CutSc_1/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "1e903005-95da-47c7-b0b2-5eacdf6f2ab3"
msgid ""
"\"Oh, the rain is gone, and the fields are green! And we owe it all to our "
"dearest king!\""
msgstr "\"오, 비가 그치고 들판이 푸르러졌어요! 그리고 우리는 이 모든 것을 우리의 "
"사랑하는 왕에게 빚지고 있습니다!\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_Trickster_CutSc_1/CommandBark1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "437503c7-2cf0-4113-9c36-5ca31d358130"
msgid "\"Morning food and midnight drink! All because of our great king!\""
msgstr "\"아침 음식과 한밤중의 음료! 모두 우리의 위대한 왕 덕분입니다!\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_Trickster_CutSc_1/CommandBark2
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "62c2aa7c-6c84-430c-bd6d-2d914a9b2a78"
msgid ""
"\"Giant moths ate my fur coat today! The king commands it, and they obey!\""
msgstr "\"오늘 거대한 나방이 내 모피 코트를 먹었어요! 왕이 명령하면 그들은 "
"순종합니다!\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_Trickster_CutSc_1/CommandBark3
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "166ebc94-6463-4912-9086-bd6c32a0083d"
msgid ""
"\"What makes the world go round and round? Our king's orders and his great "
"crown!\""
msgstr "\"무엇이 세상을 돌고 도는가? 우리 왕의 명령과 그의 위대한 왕관!\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_demonSuccubus_CutSc/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "b55ef530-02a9-4b9f-9d4e-6bfe5a20c8be"
msgid ""
"\"You are all so strong and brave. Which one of you should I reward with my "
"affection? I just can't decide...\""
msgstr "\"너희들은 모두 정말 강하고 용감해. 여러분 중 누구에게 애정을 쏟아야 할까요? "
"결정할 수 없어요...\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_demonSuccubus_CutSc/CommandBark1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "e488ca67-a58c-492b-a12e-7df901733209"
msgid ""
"\"I've killed a hundred enemies! Choose me, my lady! Or I'll fall on my "
"sword!\""
msgstr "\"나는 백 명의 적을 죽였어! 저를 선택해주세요, 아가씨! 아니면 내 칼에 "
"쓰러지겠어!\""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_demonSuccubus_CutSc/CommandBark2
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "172f914b-d602-4d29-8f42-2a5d1d779874"
msgid ""
"\"You? But you were a paladin not so long ago. Didn't you swear an oath of "
"loyalty to another lady? That displeases me.\""
msgstr "\"너? 얼마 전까지만 해도 성기사였잖아. 다른 여인에게 충성을 맹세하지 "
"않았나요? 그건 나를 불쾌하게합니다 \""

#. File = World/Cutscenes/DrezenCapital/DrezenCapital_demonSuccubus_CutSc/CommandBark3
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "4f6a334b-0865-41d5-9057-5a8ddea8464d"
msgid ""
"\"I lost my loyalty to her a long time ago! For you, my lady, I'm willing to "
"break any oaths!\""
msgstr "\"나는 오래전에 그녀에 대한 충성심을 잃었습니다! 당신을 위해서라면 어떤 "
"맹세도 기꺼이 깨뜨릴 수 있습니다!\""

#. File = World/Cutscenes/DrezenCapital/EmberQ1_PreachingInPrison/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "93e2483d-22aa-447a-9ded-a4e31fee7688"
msgid "\"Are you cold? I'll ask for more blankets for you.\""
msgstr "\"추워요? 담요를 더 가져다 드릴게요\""

#. File = World/Cutscenes/DrezenCapital/EmberQ1_PreachingInPrison/CommandBark 1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "b383d159-ef69-4e28-8aba-f5de3bdc29f4"
msgid "\"I'm so happy you're alive! Sooner or later they will let you out.\""
msgstr "\"당신이 살아 있어서 정말 기뻐요! 조만간 그들이 당신을 꺼내 줄 것입니다 \""

#. File = World/Cutscenes/DrezenCapital/EmberQ1_PreachingInPrison/CommandBark 2
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "54484973-4bd6-41d7-b0c0-60e783d7f06d"
msgid ""
"\"You made a mistake — but who is without sin? Forgive yourself and help "
"others avoid your mistake.\""
msgstr "\"당신은 실수를 저질렀지만 죄가 없는 사람은 누구입니까? 자신을 용서하고 다른 "
"사람들이 당신의 실수를 피하도록 도와주세요.\""

#. File = World/Cutscenes/DrezenCapital/EmberQ1_Sermon/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "13084530-4429-46a8-b92a-36a7545dd60b"
msgid ""
"\"We'll all die — maybe very soon. How can we waste our short lives fighting?"
"\""
msgstr "\"우리는 모두 죽을 것입니다. 아마도 곧 죽을 것입니다. 어떻게 우리의 짧은 "
"인생을 싸우느라 낭비할 수 있겠습니까?\""

#. File = World/Cutscenes/DrezenCapital/EmberQ1_Sermon/CommandBark 1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "b7721613-ccad-408e-920a-ec6d5526da1f"
msgid ""
"\"It's easy to say, 'love other people.' Have you seen the mugs on some folk?"
"\""
msgstr "\"다른 사람을 사랑하라\"고 말하기는 쉽습니다. 어떤 사람들의 머그잔을 "
"보셨나요?\""

#. File = World/Cutscenes/DrezenCapital/EmberQ1_Sermon/CommandBark 2
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "ba3369d9-afd0-4c1d-8aaa-701312d97263"
msgid "\"There is nothing more pleasant than making someone happier.\""
msgstr "\"누군가를 더 행복하게 만드는 것보다 더 즐거운 것은 없습니다\""

#. File = World/Cutscenes/DrezenCapital/EmberQ1_Sermon/CommandBark 3
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "1d99277e-9ab0-46f7-842d-9d6400e6627f"
msgid ""
"\"All of you are wonderful. Everyone in this city is wonderful and kind!\""
msgstr "\"여러분은 모두 훌륭합니다. 이 도시의 모든 사람들은 훌륭하고 친절합니다!\""

#. File = World/Cutscenes/DrezenCapital/EmberQ1_Sermon/CommandBark 4
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "f8955019-5164-4918-8c55-3b0465971497"
msgid ""
"\"If evil was done to you, don't store it in your heart. Forgive, and let it "
"go.\""
msgstr "\"당신에게 악이 행해졌다면, 그것을 마음에 저장하지 마십시오. 용서하고 "
"잊어버리세요\""

#. File = World/Cutscenes/DrezenCapital/EmberQ1_Sermon/CommandBark 5
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "35a49ca3-1003-472e-a82d-3adbfb7069fb"
msgid "\"How can we defeat the demons if we become like them?\""
msgstr "\"우리가 데몬처럼되면 어떻게 데몬을 물리 칠 수 있습니까? \""

#. File = World/Cutscenes/DrezenCapital/EmberQ1_Sermon/CommandBark 6
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "71be4432-5bd5-4e47-ae3e-bdf325fca14b"
msgid "\"What's this ragamuffin blathering on about? Boooo! Chase her away!\""
msgstr "\"이 라가머핀은 무엇에 대해 떠들고 있는 거죠? 부우! 그녀를 쫓아 내세요!\""

#. File = World/Cutscenes/DrezenCapital/EmberQ1_Sermon/CommandBark 7
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
#. 
#. File = World/Encounters/DrezenCapital/ActionsHolders/EmberQ1_SermonBarks_Actions
#. Type = Kingmaker.ElementsSystem.ActionsHolder
#. Path = Actions/Actions[0]/Actions[1]/Action/Actions[0]/WhatToBark
msgctxt "28204542-4b18-4718-9a7e-103cc800322e"
msgid "\"She speaks so beautifully — I'm tearing up...\""
msgstr "\"그녀는 너무 아름답게 말한다 - 나는 눈물을 흘리고있다...\""

#. File = World/Cutscenes/DrezenCapital/EmberQ1_Sermon/CommandBark 8
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
#. 
#. File = World/Encounters/DrezenCapital/ActionsHolders/EmberQ1_SermonBarks_Actions
#. Type = Kingmaker.ElementsSystem.ActionsHolder
#. Path = Actions/Actions[0]/Actions[0]/Action/Actions[0]/WhatToBark
msgctxt "1e62bfdf-158e-4dfc-b315-b95f72b0f1cd"
msgid "\"The girl has a way with words, I like listening to her!\""
msgstr "\"그 소녀는 말을 하는 방법이 있어, 나는 그녀의 말을 듣는 것을 좋아합니다!\""

#. File = World/Cutscenes/DrezenCapital/EmberQ1_Sermon/CommandBark 9
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "66e88958-d1ce-446c-942c-81f9a31dc949"
msgid ""
"\"Right! Now you're talking! Heh, if anyone told me that ten years ago...\""
msgstr "\"맞아요! 이제야 말하네! 허, 10년 전에 누가 나한테 그렇게 말했으면...\""

#. File = World/Cutscenes/DrezenCapital/HappyHilor/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "caa6bc79-b190-49da-a4e0-01c5251fff3d"
msgid ""
"\"Lourry! My sweet girl, I haven't seen you in three years! We have so much "
"catching up to do!\""
msgstr "\"로리! 내 사랑스러운 딸, 3년 동안 못 봤어! 우린 할 일이 너무 많아!\""

#. File = World/Cutscenes/DrezenCapital/HotelIndoorPercCheckFailed_scene/CommandAction
#. Type = Kingmaker.AreaLogic.Cutscenes.CommandAction
#. Path = Action/Actions[0]/WhatToBark
msgctxt "7585d046-55c8-440e-a746-9f412eb2a50b"
msgid "\"...and then he died, and no one knows...\""
msgstr "\"...그리고 그는 죽었고, 아무도 모른다...\""

#. File = World/Cutscenes/DrezenCapital/HotelIndoorPercCheckFailed_scene/CommandAction 1
#. Type = Kingmaker.AreaLogic.Cutscenes.CommandAction
#. Path = Action/Actions[0]/WhatToBark
msgctxt "44a52131-599d-4220-94a8-b50f1b038275"
msgid "\"...they danced there, savages, and worshiped the trees...\""
msgstr "\"...그들은 그곳에서 춤을 추고 야만인들이 나무를 숭배했습니다...\""

#. File = World/Cutscenes/DrezenCapital/HotelIndoorPercCheckFailed_scene/CommandAction 2
#. Type = Kingmaker.AreaLogic.Cutscenes.CommandAction
#. Path = Action/Actions[0]/WhatToBark
msgctxt "761ecdb3-9596-428a-8dfa-e8a2cf53347c"
msgid "\"...looks like a lie. Like a story for kids...\""
msgstr "\"...거짓말처럼 보입니다. 아이들을 위한 이야기처럼...\""

#. File = World/Cutscenes/DrezenCapital/HotelIndoorPercCheckFailed_scene/CommandAction 3
#. Type = Kingmaker.AreaLogic.Cutscenes.CommandAction
#. Path = Action/Actions[0]/WhatToBark
msgctxt "2982abc2-0cc9-4ef7-8814-ad9b0c29bf9c"
msgid ""
"\"...they weren't fools. When it got hot, they collected everything...\""
msgstr "\"...그들은 바보가 아니었다. 더워지면 그들은 모든 것을 모았습니다...\""

#. File = World/Cutscenes/DrezenCapital/HotelIndoorPercCheckFailed_scene/CommandAction 4
#. Type = Kingmaker.AreaLogic.Cutscenes.CommandAction
#. Path = Action/Actions[0]/WhatToBark
msgctxt "eb606771-fca7-428c-bc45-56acad35c4da"
msgid "\"…or something worse. It is dangerous to go there...\""
msgstr "\"...아니면 더 나쁜 것. 그곳에 가는 것은 위험합니다...\""

#. File = World/Cutscenes/DrezenCapital/HotelIndoorPercCheckFailed_scene/CommandAction 5
#. Type = Kingmaker.AreaLogic.Cutscenes.CommandAction
#. Path = Action/Actions[0]/WhatToBark
msgctxt "fd782a2b-f927-4297-8d6e-8402046bd7f4"
msgid "\"...Of course it's dangerous. That's why we offer...\""
msgstr "\"...당연히 위험합니다. 그래서 우리는...\""

#. File = World/Cutscenes/DrezenCapital/HotelIndoorPercCheckPassed_scene/CommandAction
#. Type = Kingmaker.AreaLogic.Cutscenes.CommandAction
#. Path = Action/Actions[0]/WhatToBark
msgctxt "ac11f561-24b7-4bad-8087-48ba02bb4dca"
msgid ""
"\"...one old Kellid told this my brothers and me on his deathbed, and then "
"he died, and no one knows but us...\""
msgstr ""
"\"...켈리드라는 노인이 임종을 앞두고 저와 제 형제들에게 이 얘기를 해줬고, "
"그분은 돌아가셨는데 우리 말고는 아무도 몰라요...\""

#. File = World/Cutscenes/DrezenCapital/HotelIndoorPercCheckPassed_scene/CommandAction 1
#. Type = Kingmaker.AreaLogic.Cutscenes.CommandAction
#. Path = Action/Actions[0]/WhatToBark
msgctxt "72230321-916c-48a7-961b-bc90703e5a11"
msgid ""
"\"...I know this is their sacred forest. They prayed in the Meadow of the "
"Spirits...\""
msgstr "\"...나는 이곳이 그들의 신성한 숲이라는 것을 안다. 그들은 정령의 초원에서 "
"기도했다...\""

#. File = World/Cutscenes/DrezenCapital/HotelIndoorPercCheckPassed_scene/CommandAction 2
#. Type = Kingmaker.AreaLogic.Cutscenes.CommandAction
#. Path = Action/Actions[0]/WhatToBark
msgctxt "b0851326-f4e4-42cd-8123-def514070666"
msgid "\"...shamanic relics, you say? A big treasure? It sounds like a lie…\""
msgstr "\"...주술적 유물이라고요? 큰 보물? 거짓말처럼 들리는데...\""

#. File = World/Cutscenes/DrezenCapital/HotelIndoorPercCheckPassed_scene/CommandAction 3
#. Type = Kingmaker.AreaLogic.Cutscenes.CommandAction
#. Path = Action/Actions[0]/WhatToBark
msgctxt "b8722ad9-e5d5-431e-9cef-09140fc84fec"
msgid ""
"\"...everything was collected, carried away and buried in that cave...\""
msgstr "\"...모든 것이 수집되어 운반되어 그 동굴에 묻혔습니다...\""

#. File = World/Cutscenes/DrezenCapital/HotelIndoorPercCheckPassed_scene/CommandAction 4
#. Type = Kingmaker.AreaLogic.Cutscenes.CommandAction
#. Path = Action/Actions[0]/WhatToBark
msgctxt "c3138ead-dff9-4d53-a6d3-52f081b56935"
msgid ""
"\"...the scouts say a bear lives in those forests, a terrible one. And there "
"must be magic guards in the cave...\""
msgstr "\"...정찰병들이 저 숲에 끔찍한 곰이 산다고 했어요. 그리고 동굴에 마법 "
"경비병이 있을 거야...\""

#. File = World/Cutscenes/DrezenCapital/HotelIndoorPercCheckPassed_scene/CommandAction 5
#. Type = Kingmaker.AreaLogic.Cutscenes.CommandAction
#. Path = Action/Actions[0]/WhatToBark
msgctxt "97bed17c-30b4-48ef-8f57-a234fc93667f"
msgid ""
"\"...that's why we suggest you come with us. We'll split the treasure "
"equally...\""
msgstr "\"...그래서 우리와 함께 가자고 제안하는 거야. 보물을 공평하게 나누자...\""

#. File = World/Cutscenes/DrezenCapital/MorvegVSCiar_Bark1_scene/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "258da862-978a-4241-b5ce-649e08fc09bd"
msgid ""
"\"You're still green, Morveg, I'm afraid you'll make a mistake we all "
"regret. You lack discipline. And I've told you enough times — address me as "
"'Sir Ciar' in public, in keeping with my rank.\""
msgstr ""
"\"넌 아직 풋내기야, 모베그, 우리 모두가 후회할 실수를 할까봐 걱정이다. 자넨 "
"규율이 부족해. 그리고 내가 여러번 말했듯이, 내 계급에 맞게 공공장소에서는 "
"나를 '시어 경'으로 불러라.\""

#. File = World/Cutscenes/DrezenCapital/MorvegVSCiar_Bark1_scene/CommandBark 1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "04dd969e-77f6-4045-830c-d48dd9086af4"
msgid "\"Ciar, you said you were going to send me with the scouts...\""
msgstr "\"시아르, 정찰대와 함께 날 보낸다고 했잖아...\""

#. File = World/Cutscenes/DrezenCapital/MorvegVSCiar_Bark2_scene/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "059bd50a-dba5-4781-8b32-882cd99cd5c1"
msgid ""
"\"Morveg, a little bird told me that you were seen in a tavern recently. I "
"hope you remember that liquor is strictly prohibited in the order?\""
msgstr ""
"\"모베그, 작은 새가 당신이 최근에 선술집에서 목격되었다고 말해줬어요. "
"주문에서 술은 엄격히 금지되어 있다는 것을 기억하길 바랍니다.\""

#. File = World/Cutscenes/DrezenCapital/MorvegVSCiar_Bark2_scene/CommandBark 1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "e74442ce-f4c5-4a0f-912d-0bf91f89d06b"
msgid ""
"\"Rubbish! They must've taken me for someone else! You know I take all my "
"vows seriously. I'm a soldier, I don't have time for fun.\""
msgstr ""
"\"쓰레기! 날 다른 사람으로 착각한 게 틀림없어! 내가 모든 서약을 진지하게 "
"받아들이는 거 알잖아. 난 군인이야, 놀 시간 없어.\""

#. File = World/Cutscenes/DrezenCapital/MorvegVSCiar_Bark3_scene/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "4536c544-f8dd-4b55-9880-055ac2eb9da2"
msgid ""
"\"When are the troops arriving? And the Queen? And when's the attack going "
"to start?\""
msgstr "\"군대는 언제 도착하나요? 여왕은요? 공격은 언제 시작하죠?\""

#. File = World/Cutscenes/DrezenCapital/MorvegVSCiar_Bark3_scene/CommandBark 1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "ab1ee620-2fb2-4303-a4bc-c0844af6cd85"
msgid ""
"\"Morveg, hurrying in war only gives you the right to die first. You have to "
"learn to be patient.\""
msgstr "\"모베그, 전쟁에서 서두르면 먼저 죽을 권리만 얻게 돼요. 인내하는 법을 배워야 "
"해.\""

#. File = World/Cutscenes/DrezenCapital/MythicLich_SeptimusAndElyankaDispute01/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "ef6e1e13-ae44-4a70-9735-5d67a62a1ce8"
msgid ""
"\"But you agree that an undead lord is free of vices that mortals have, "
"don't you?\""
msgstr "\"하지만 언데드 군주에게는 인간에게 있는 악덕이 없다는데 동의하지 않습니까?\""

#. File = World/Cutscenes/DrezenCapital/MythicLich_SeptimusAndElyankaDispute01/CommandBark 1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "bf7577eb-ee49-4890-a936-293363df7ba6"
msgid "\"This is so very fair, reserved, and wise!\""
msgstr "\"정말 공정하고, 신중하고, 현명하네요!\""

#. File = World/Cutscenes/DrezenCapital/MythicLich_SeptimusAndElyankaDispute01/CommandBark 2
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "f6564e10-f699-4c69-9489-dfbbc15a3720"
msgid "\"Exactly. Forcibly raising mortals, screaming in horror, as undead!\""
msgstr "\"바로 그거야. 인간을 강제로 언데드로 키우며 공포에 질려 비명을 지르고 있죠!"
"\""

#. File = World/Cutscenes/DrezenCapital/MythicLich_SeptimusAndElyankaDispute01/CommandBark 3
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "7ee34333-77c3-48c7-a6dd-f8d6af208317"
msgid ""
"\"He is wise, reserved, fair, doesn't worry about dying or leaving an "
"inheritance!\""
msgstr "\"그는 현명하고, 내성적이며, 공정하고, 죽거나 유산을 남기는 것에 대해 "
"걱정하지 않습니다!\""

#. File = World/Cutscenes/DrezenCapital/MythicLich_SeptimusAndElyankaDispute02/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "eb1a97b2-c202-4054-8783-c3171cd4c956"
msgid ""
"\"The way you cling to the remains of mortal life is disgusting! You've been "
"given eternity!\""
msgstr "\"인간 생명의 잔해에 집착하는 너희의 모습은 역겨워! 너희에게는 영원이 "
"주어졌어!\""

#. File = World/Cutscenes/DrezenCapital/MythicLich_SeptimusAndElyankaDispute02/CommandBark 1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "5c92f09d-e416-467c-8347-36d176f5acd0"
msgid "\"We just don't want to turn into deadly bloodthirsty monsters.\""
msgstr "\"피에 굶주린 치명적인 괴물로 변하고 싶지 않을 뿐입니다.\""

#. File = World/Cutscenes/DrezenCapital/MythicLich_SeptimusAndElyankaDispute02/CommandBark 2
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "321491a6-d54b-4a8c-a1c1-edf31bbde7e3"
msgid ""
"\"Listening to you, you regret becoming an undead. Are you ashamed, boy?\""
msgstr "\"네 말을 들으니 언데드가 된 걸 후회하는구나. 부끄럽지 않니, 꼬마야?\""

#. File = World/Cutscenes/DrezenCapital/MythicLich_SeptimusAndElyankaDispute02/CommandBark 3
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "86849bec-46c4-41c7-8277-a74259f81fec"
msgid "\"I'm hundreds of years older than you, girl. Have some respect.\""
msgstr "\"난 너보다 수백 년은 더 늙었어. 예의를 갖춰야지.\""

#. File = World/Cutscenes/DrezenCapital/MythicLich_SeptimusAndElyankaDispute03/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "d71cf1de-d5bf-45b7-b34f-b10530a4ddad"
msgid ""
"\"Urgathoa orders us to throw off our fetters and enjoy ourselves! Don't you "
"in Geb respect her will?\""
msgstr "\"우르가쏘아가 족쇄를 벗어던지고 즐기라고 명령하셨어요! 게브는 그녀의 뜻을 "
"존중하지 않나요?\""

#. File = World/Cutscenes/DrezenCapital/MythicLich_SeptimusAndElyankaDispute03/CommandBark 1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "816a7c2e-7ae2-4459-bbd4-55163925bb5c"
msgid ""
"\"I'm not speaking for all the people of Geb, just for the ones who share my "
"opinion.\""
msgstr "\"저는 게브의 모든 사람들을 대변하는 것이 아니라 제 의견에 동의하는 "
"사람들만을 대변합니다.\""

#. File = World/Cutscenes/DrezenCapital/MythicLich_SeptimusAndElyankaDispute03/CommandBark 2
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "2fadc0f4-cc3f-4afe-975b-8bcd5fb5325c"
msgid ""
"\"But I think you lack discipline and restraint. This is why they are afraid "
"and don't want to listen to you.\""
msgstr "\"하지만 자제력과 절제력이 부족한 것 같아요. 그래서 그들은 두려워하고 당신의 "
"말을 듣지 않으려 합니다.\""

#. File = World/Cutscenes/DrezenCapital/MythicLich_SeptimusAndElyankaDispute03/CommandBark 3
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "b534be16-56cc-4a87-a667-e037cea63cda"
msgid ""
"\"I don't need menials to agree with me. Absolute obedience is enough.\""
msgstr "\"내 의견에 동의해줄 부하직원은 필요 없습니다. 절대적인 순종으로 충분합니다."
"\""

#. File = World/Cutscenes/DrezenCapital/MythicLich_UrgathoaPatrols/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "c9725763-47e0-402e-ad53-47c94a29158b"
msgid ""
"\"The living, give way to warriors of Death. Our swords thirst for the blood "
"of demons and criminals.\""
msgstr "\"산 자들은 죽음의 전사들에게 양보하라. 우리의 검은 데몬과 범죄자의 피를 "
"갈망한다.\""

#. File = World/Cutscenes/DrezenCapital/MythicLich_UrgathoaPatrols/CommandBark 1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "0e0fbf4d-b13f-475a-9b97-0713f51f1371"
msgid ""
"\"We are sleepless guards. We can't be bribed or threatened or distracted. "
"We march toward death and are glad of it.\""
msgstr ""
"\"우리는 잠들지 않는 경비원입니다. 우리는 뇌물을 받거나 협박을 받거나 주의를 "
"분산시킬 수 없습니다. 우리는 죽음을 향해 행진하며 그 죽음을 기뻐합니다.\""

#. File = World/Cutscenes/DrezenCapital/MythicLich_UrgathoaPatrols/CommandBark 2
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "abdb0b21-b610-4206-8200-c480c3089755"
msgid ""
"\"Enjoy yourselves while you are alive, mortals! Eat, drink, have fun! For "
"we will all die! Thus speaks Urgathoa!\""
msgstr "\"인간들아, 살아 있는 동안 즐겨라! 먹고, 마시고, 즐겨라! 우리는 모두 죽을 "
"것이니! 우르가쏘아가 이렇게 말한다!\""

#. File = World/Cutscenes/DrezenCapital/MythicLich_VampirePatrols/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "6e7cb85d-d705-4f03-b83c-e42c51354e44"
msgid ""
"\"Don't be afraid of us. We are undead knights who have come to fight demons."
"\""
msgstr "\"우릴 두려워하지 마세요. 우리는 데몬과 싸우러 온 언데드 기사입니다.\""

#. File = World/Cutscenes/DrezenCapital/MythicLich_VampirePatrols/CommandBark 1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "6ae60a87-65f7-4d3e-8425-d3ef341c20d8"
msgid ""
"\"I feel hunger. But do not be afraid, full-blooded. I am not a wild animal "
"out to hunt you!\""
msgstr "\"배고픔을 느낍니다. 하지만 두려워하지 마라, 혈기왕성한 자식아. 나는 너희를 "
"사냥하러 나온 야생 동물이 아니니까!\""

#. File = World/Cutscenes/DrezenCapital/MythicLich_VampirePatrols/CommandBark 2
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "3b66f573-e71f-4c7b-a0a2-c07a2fe319e9"
msgid ""
"\"We are the warriors of the night. While you sleep, we guard you from the "
"terrors of the Worldwound.\""
msgstr "\"우리는 밤의 전사들입니다. 여러분이 잠든 동안 세계상처의 공포로부터 "
"여러분을 지켜드립니다.\""

#. File = World/Cutscenes/DrezenCapital/MythicShadow_AfterCombat/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "0dfbb83f-59e7-4cd7-8363-592c980580a1"
msgid "\"Why... why... It hurts so much...\""
msgstr "\"왜.. 왜.. 왜.. 너무 아파...\""

#. File = World/Cutscenes/DrezenCapital/MythicShadow_Appears/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "9e65eccf-19c8-4f2f-8226-3ea3e4e8bd2e"
msgid "\"Who are you? Who are you?! Why are you tormenting me?\""
msgstr "\"넌 누구야? 넌 누구야?! 왜 나를 괴롭히는거야?\""

#. File = World/Cutscenes/DrezenCapital/Theatre_PonyMoveOnPoint/CommandAction
#. Type = Kingmaker.AreaLogic.Cutscenes.CommandAction
#. Path = Action/Actions[0]/IfTrue/Actions[0]/WhatToBark
msgctxt "f9497177-8281-4381-8db6-4d18893c74c9"
msgid "\"Ouch!\""
msgstr "\"아야!\""

#. File = World/Cutscenes/DrezenCapital/Ziggurat_CiarAttack/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "e99c4e80-d39d-46ec-91a9-9ad4716174ef"
msgid "\"The Commander betrayed us! Destroy {mf|him|her}!\""
msgstr "\"사령관이 우릴 배신했다! {mf|그|그녀}를 파괴해라!\""

#. File = World/Cutscenes/DrezenCapital/Ziggurat_CiarAttack/CommandBark 1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "599a4af2-2f8f-4e72-b4f6-0a0e37018313"
msgid ""
"\"The light shall be ever bright! Fight for Ciar! Fight for poor Morveg!\""
msgstr "\"빛은 영원히 밝을 것이다! 시아르를 위해 싸워라! 불쌍한 모베그를 위해 싸워라!"
"\""

#. File = World/Cutscenes/DrezenCapital/Ziggurat_CiarAttack/CommandBark 2
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "0f063b26-7725-4417-b28b-5ae4aaf63fb9"
msgid ""
"\"We're crusaders! Our souls are pure! Evil shall not find its way into our "
"midst!\""
msgstr "\"우리는 십자군입니다! 우리의 영혼은 순수하다! 악은 우리 가운데 들어올 수 "
"없다!\""

#. File = World/Cutscenes/DrezenCapital/Ziggurat_SkeletonBarks/CommandBark
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "4c23d02c-0363-412f-8ed6-2639e1442390"
msgid "\"Blood... I'm thirsty...\""
msgstr "\"피.. 목이 마르다...\""

#. File = World/Cutscenes/DrezenCapital/Ziggurat_SkeletonBarks/CommandBark 1
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "34953b53-0d85-4b49-8ee7-8b67c0a0aee6"
msgid "\"Kill the leeches!\""
msgstr "\"거머리를 죽여라!\""

#. File = World/Cutscenes/DrezenCapital/Ziggurat_SkeletonBarks/CommandBark 2
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "6b1f9d45-8a1f-42fb-9916-80d8bb88fa4c"
msgid "\"Glory to Urgathoa!\""
msgstr "\"우르가쏘아에게 영광을!\""

#. File = World/Cutscenes/DrezenCapital/Ziggurat_SkeletonBarks/CommandBark 3
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "fac83543-2fa0-405c-be1d-2ef037e39a0e"
msgid "\"Find the full-blooded. Kill the full-blooded.\""
msgstr "\"혈육을 찾아라. 완전한 피를 가진 자를 죽여라.\""

#. File = World/Cutscenes/DrezenCapital/Ziggurat_SkeletonBarks/CommandBark 4
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "fc6364b6-4ff8-4db7-a01c-eedf03c876f9"
msgid "\"Withered bones. We cannot eat those! We want blood!\""
msgstr "\"시든 뼈. 그건 못 먹어! 피를 원한다!\""

#. File = World/Cutscenes/DrezenCapital/Ziggurat_SkeletonBarks/CommandBark 5
#. Type = Kingmaker.AreaLogic.Cutscenes.Commands.CommandBark
#. Path = SharedText
msgctxt "d136d39b-e3c0-4d8a-82e5-86b6656760cb"
msgid "\"We'll repair you. We'll fix you.\""
msgstr "\"우리가 고쳐드리겠습니다. 우리가 고쳐드리겠습니다.\""

