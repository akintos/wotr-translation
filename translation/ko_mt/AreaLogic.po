#. Type = Kingmaker.AreaLogic.Cutscenes.CommandAction
#. Path = Action/Actions[0]/Actions[4]/Action/Actions[0]/WhatToBark
msgctxt "1aefbae2-c91b-4b7f-a792-f1583dcf7a7a"
msgid "\"We were here... We lived here...\""
msgstr "\"우리는 여기에 있었다... 우린 여기서 살았어...\""

#. File = Buffs/DLC5/ShadowPlane/DLC5_LC_CombatBuff
#. Type = Kingmaker.UnitLogic.Buffs.Blueprints.BlueprintBuff
#. Path = Components[0]/Actions/Actions[0]/Actions[1]/Action/Actions[0]/WhatToBark
#. 
