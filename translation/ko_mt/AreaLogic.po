#. Type = Kingmaker.AreaLogic.Cutscenes.CommandAction
#. Path = Action/Actions[0]/Actions[2]/Action/Actions[0]/WhatToBark
#. 
#. File = World/Cutscenes/DLC5_LostChapel_PlaneOfShadow/DLC5_LC_ShadowsCutscene/CommandAction1
#. Type = Kingmaker.AreaLogic.Cutscenes.CommandAction
#. Path = Action/Actions[0]/Actions[3]/Action/Actions[0]/WhatToBark
msgctxt "76986c37-4f3f-44c5-82ee-fc1ef344e330"
msgid "\"There's nothing left... Only pain remains...\""
msgstr "\"아무것도 남지 않았습니다... 고통만 남았어...\""

#. File = Buffs/DLC5/ShadowPlane/DLC5_LC_CombatBuff
#. Type = Kingmaker.UnitLogic.Buffs.Blueprints.BlueprintBuff
#. Path = Components[0]/Actions/Actions[0]/Actions[2]/Action/Actions[0]/WhatToBark
