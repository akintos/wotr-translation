msgctxt "2441f1ac-961e-4a6e-b114-5e4439f95437"
msgid "Silver Holy Symbol of Gozreh"
msgstr "고즈레의 은색 성스러운 상징"

msgctxt "0e865716-b6f0-4380-8170-28fe829e60d2"
msgid "Silver Holy Symbol of Groetus"
msgstr "그로 투스의 은색 성스러운 상징"

msgctxt "7785130c-db98-4647-80db-d905e264ff71"
msgid "Silver Holy Symbol of Gyronna"
msgstr "기로나의 은색 성스러운 상징"

msgctxt "d9ade1d6-9a86-4850-9c6f-99017b1d659f"
