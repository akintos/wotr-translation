msgid "Eruption"
msgstr ""

#. Type = Kingmaker.Blueprints.Classes.BlueprintFeature
#. Path = EruptionInfusion::m_Description
#. 
#. Type = Kingmaker.UnitLogic.Abilities.Blueprints.BlueprintAbility
#. Path = EruptionBlueFlameBlastAbility::m_Description
#. 
#. Type = Kingmaker.UnitLogic.Abilities.Blueprints.BlueprintAbility
#. Path = EruptionFireBlastAbility::m_Description
#. 
