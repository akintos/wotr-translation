msgid "Dimension Strike"
msgstr ""

#. Type = Kingmaker.Blueprints.Classes.BlueprintFeature
#. Path = DimensionStrikeFeature::m_Description
#. 
#. Type = Kingmaker.Blueprints.Classes.BlueprintFeature
#. Path = EldritchDimensionStrikeFeature::m_Description
#. 
